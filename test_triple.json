8fdb\u5165\u70b9\u7403\u5927\u6218\uff0c\u4e0a\u6e2f\u70b9\u74035\u6bd43\u8d62\u51fa\uff0c\u6700\u7ec8\u4e24\u961f\u6b64\u6218\u6218\u62106\u6bd44\uff0c\u4e24\u56de\u5408\u603b\u6bd4\u5206\u4e0a\u6e2f7\u6bd45\u6218\u80dc\u5168\u5317\u73b0\u4ee3\uff0c\u6740\u51652019\u4e9a\u51a0\u516b\u5f3a\u3002', 'spo_list': [('\u5317\u4eac\u65f6\u95f46\u670826\u65e5\u665a6\u70b9', '\u6218\u80dc', '2019\u5e74\u4e9a\u51a01/8\u51b3\u8d5b\u7b2c\u4e8c\u56de\u5408')]}, {'text': ' 6\u670819\u65e5 \uff0c \u5e02\u59d4\u4e66\u8bb0\u3001\u5e02\u4eba\u5927\u5e38\u59d4\u4f1a\u4e3b\u4efb\u4f58\u6625\u660e \u4f1a\u89c1\u4e86\u4e0a\u6d77\u5e02\u6ee8\u5dde\u5546\u4f1a\u4f1a\u957f\u738b\u8fc5\u4e00\u884c\u3002', 'spo_list': [('6\u670819\u65e5', '\u4f1a\u89c1', '\u5e02\u59d4\u4e66\u8bb0\u3001\u5e02\u4eba\u5927\u5e38\u59d4\u4f1a\u4e3b\u4efb\u4f58\u6625\u660e')]}, {'text': ' \u7814\u7a76\u5458\u674e\u4e2d\u534e \u4ecb\u7ecd\uff0c\u4e0a\u4e16\u7eaa70\u5e74\u4ee3\uff0c \u65f6\u4efb\u91cd\u5e86\u5e02\u59d4\u4e66\u8bb0\u9c81\u5927\u4e1c\u66fe\u5411\u4e0a\u7ea7\u8bf7\u793a\u6b64\u5904\u5893\u5730\u4f5c\u4f55\u5904\u7f6e\uff0c\u540e\u6765\u6ca1\u6709\u4e0b\u6587 ', 'spo_list': [('\u7814\u7a76\u5458\u674e\u4e2d\u534e', '\u4ecb\u7ecd', '\u65f6\u4efb\u91cd\u5e86\u5e02\u59d4\u4e66\u8bb0\u9c81\u5927\u4e1c\u66fe\u5411\u4e0a\u7ea7\u8bf7\u793a\u6b64\u5904\u5893\u5730\u4f5c\u4f55\u5904\u7f6e\uff0c\u540e\u6765\u6ca1\u6709\u4e0b\u6587')]}, {'text': ' \u56fd\u5bb6\u4fe1\u8bbf\u5c40 \u7684\u901a\u77e5\u6307\u51fa\uff0c\u4e3a\u5168\u9762\u843d\u5b9e \u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u80ba\u708e\u75ab\u60c5\u8054\u9632\u8054\u63a7\u63aa\u65bd \uff0c\u6700\u5927\u9650\u5ea6\u51cf\u5c11\u4eba\u5458\u6d41\u52a8\u805a\u96c6\uff0c\u4fdd\u62a4\u4eba\u6c11\u7fa4\u4f17\u751f\u547d\u5b89\u5168\uff0c\u6839\u636e\u515a\u4e2d\u592e\u9632\u63a7\u5de5\u4f5c\u51b3\u7b56\u90e8\u7f72\u548c\u5317\u4eac\u5e02\u6709\u5173\u9632\u63a7\u5b89\u6392\uff0c\u5373\u65e5\u8d77\uff0c\u4e2d\u592e\u548c\u56fd\u5bb6\u673a\u5173\u7fa4\u4f17\u6765\u8bbf\u63a5\u5f85\u573a\u6240\u6682\u65f6\u5173\u95ed\uff0c\u505c\u6b62\u63a5\u5f85\u7fa4\u4f17\u6765\u8bbf\uff0c\u79ef\u6781\u5021\u5bfc\u7fa4\u4f17\u901a\u8fc7\u7f51\u4e0a\u6295\u8bc9\u548c\u5199\u4fe1\u7b49\u65b9\u5f0f\u53cd\u6620\u8bc9\u6c42', 'spo_list': [('\u56fd\u5bb6\u4fe1\u8bbf\u5c40', '\u843d\u5b9e', '\u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u80ba\u708e\u75ab\u60c5\u8054\u9632\u8054\u63a7\u63aa\u65bd')]}, {'text': ' \u664b\u4e2d\u51ef\u901a\u6c7d\u8f66\u9500\u552e\u670d\u52a1\u6709\u9650\u516c\u53f8 \u6210\u7acb\u4e8e 2009\u5e749\u6708 \uff0c\u662f\u5e7f\u6c7d\u4e30\u7530\u5728\u5c71\u897f\u664b\u4e2d\u8bbe\u7acb\u7684\u7b2c\u4e00\u5bb6\u96c6\u6574\u8f66\u9500\u552e\u3001\u7279\u7ea6\u7ef4\u4fee\u3001\u96f6\u90e8\u4ef6\u4f9b\u5e94\u3001\u4fe1\u606f\u53cd\u9988\u4e3a\u4e00\u4f53\u7684\u4e13\u4e1a\u5316\u6c7d\u8f66\u9500\u552e\u670d\u52a1\u4f01\u4e1a', 'spo_list': [('\u664b\u4e2d\u51ef\u901a\u6c7d\u8f66\u9500\u552e\u670d\u52a1\u6709\u9650\u516c\u53f8', '\u6210\u7acb\u65e5\u671f', '2009\u5e749\u6708')]}, {'text': ' \u4e07\u4e1c\u9547 \u8363\u83b7 \u4e07\u76db\u201c\u4e2d\u534e\u9b42\u201d\u6f14\u8bb2\u6bd4\u8d5b\u4e8c\u7b49\u5956 ', 'spo_list': [('\u4e07\u4e1c\u9547', '\u8363\u83b7', '\u4e07\u76db\u201c\u4e2d\u534e\u9b42\u201d\u6f14\u8bb2\u6bd4\u8d5b\u4e8c\u7b49\u5956')]}, {'text': ' \u56fd\u4e52\u5973\u56e2 \u6536\u83b7 \u4e9a\u9526\u8d5b \u4e03\u8fde\u51a0\u76f4\u901a\u4e1c\u4eac', 'spo_list': [('\u56fd\u4e52\u5973\u56e2', '\u4e03\u8fde\u51a0', '\u4e9a\u9526\u8d5b')]}, {'text': '\u9f50\u9c81\u7f51\u6ee8\u5dde8\u670820\u65e5\u8baf\u8bb0\u8005\u4ece\u6ee8\u5dde\u5e02\u535a\u5174\u53bf\u5415\u827a\u9547\u653f\u5e9c\u83b7\u6089\uff0c 8\u670820\u65e5\u4e0a\u53489\u70b950\u5206\u8bb8 \uff0c \u5c71\u4e1c\u5b8f\u8fdc\u65b0\u6750\u6599\u79d1\u6280\u6709\u9650\u516c\u53f8\u9505\u7089\u8f66\u95f4\u5185 \u56e0\u5bfc\u70ed\u6cb9\u9600\u95e8\u6cc4\u6f0f\uff0c\u5bfc\u70ed\u6cb9\u53d1\u751f\u81ea\u71c3\uff0c\u4f01\u4e1a\u7b2c\u4e00\u65f6\u95f4\u62a5\u8b66\uff0c\u6ee8\u5dde\u5e02\u535a\u5174\u53bf\u6d88\u9632\u6551\u63f4\u5927\u961f\u51fa\u52a8\u6d88\u9632\u8f66\u56db\u8f86\uff0c\u622a\u6b6210\u70b930\u5206\uff0c\u706b\u52bf\u5df2\u88ab\u6251\u706d\uff0c\u65e0\u4eba\u5458\u4f24\u4ea1\u3002', 'spo_list': [('\u5c71\u4e1c\u5b8f\u8fdc\u65b0\u6750\u6599\u79d1\u6280\u6709\u9650\u516c\u53f8\u9505\u7089\u8f66\u95f4\u5185', '\u81ea\u71c3', '8\u670820\u65e5\u4e0a\u53489\u70b950\u5206\u8bb8')]}, {'text': '\u519b\u4e8b\u7b56\u7565 \u97e9\u6853\u60e0\u738b \uff0c \u97e9\u5398\u738b \u4e4b\u5b50', 'spo_list': [('\u97e9\u6853\u60e0\u738b', '\u7236\u4eb2', '\u97e9\u5398\u738b')]}, {'text': '9\u670826\u65e5\u6d88\u606f\uff0c\u6570\u5b57\u8d27\u5e01\u677f\u5757\u4e34\u8fd1\u6536\u76d8\u7ee7\u7eed\u91cd\u632b\uff0c\u622a\u81f3\u53d1\u7a3f\uff0c \u56db\u65b9\u7cbe\u521b \u3001 \u6d77\u8054\u91d1\u6c47 \u3001\u98de\u5929\u8bda\u4fe1\u3001\u6570\u5b57\u8ba4\u8bc1\u7b49\u591a\u80a1\u8dcc\u505c\uff0c\u91d1\u51a0\u80a1\u4efd\u3001\u5fa1\u94f6\u80a1\u4efd\u3001\u5965\u9a6c\u7535\u5668\u7b49\u8dcc\u903e8%\u3002', 'spo_list': [('\u56db\u65b9\u7cbe\u521b', '\u8dcc\u505c', '\u6d77\u8054\u91d1\u6c47')]}, {'text': ' hiv\u611f\u67d3 @\u5bf9\u4e8e\u6709\u75c7\u72b6\u7684 \u7537\u7537\u6027\u884c\u4e3a\u8005 \uff0c\u4f9d\u636e\u4e2a\u4f53\u5371\u9669\u56e0\u7d20\u3001\u5f53\u5730\u7684\u6d41\u884c\u75c5\u5b66\u548c\u5f53\u5730\u7684\u653f\u7b56\uff0c\u53ef\u4ee5\u8003\u8651\u8fdb\u884c\u66f4\u9891\u7e41\u7684\u76d1\u6d4b\u3002 hiv\u611f\u67d3 @\u5c3d\u7ba1\u6709\u8fd9\u4e00\u63a8\u8350\uff0c\u4f46\u662f37%\u7684\u53d8\u6027\u7537\u3001\u5973\u4ece\u672a\u63a5\u53d7\u8fc7hiv\u68c0\u6d4b\u3002', 'spo_list': [('hiv\u611f\u67d3', '\u7b5b\u67e5', '\u7537\u7537\u6027\u884c\u4e3a\u8005')]}, {'text': '3\u670825\u65e5\uff0c \u6b66\u6c49\u5e02117\u6761\u516c\u4ea4\u7ebf\u8def \u6062\u590d \u8fd0\u8425 \uff0c28\u65e5\uff0c6\u6761\u8f68\u9053\u4ea4\u901a\u7ebf\u8def\u5f00\u59cb\u901a\u884c', 'spo_list': [('\u6b66\u6c49\u5e02117\u6761\u516c\u4ea4\u7ebf\u8def', '\u6062\u590d', '\u8fd0\u8425')]}, {'text': '\u201d8\u670826\u65e5\u665a\uff0c  \u5468\u6069\u6765  \u5728 \u533b\u9662 \u4f1a\u89c1 \u5373\u5c06\u8fd4\u56de\u67ec\u57d4\u5be8\u7684\u897f\u54c8\u52aa\u514b\u4eb2\u738b\u548c\u5bbe\u52aa\u4eb2\u738b ', 'spo_list': [('\u5468\u6069\u6765', '\u4f1a\u89c1', '\u5373\u5c06\u8fd4\u56de\u67ec\u57d4\u5be8\u7684\u897f\u54c8\u52aa\u514b\u4eb2\u738b\u548c\u5bbe\u52aa\u4eb2\u738b'), ('\u5468\u6069\u6765', '\u5730\u70b9', '\u533b\u9662')]}, {'text': '\u8bb8\u6674\u4f4e\u80f8\u7eb1\u88d9\u6d77\u8fb9\u5408\u5f71\uff0c\u621b\u7eb3\u65f6\u95f45\u670824\u65e5\uff0c \u5f90\u6d69\u5cf0\u5bfc\u6f14\u65b0\u6b66\u4fa0\u5de8\u5236\u300a\u5200\u80cc\u85cf\u8eab\u300b \u5728\u621b\u7eb3\u4e3e\u884c\u5b9a\u6863\u53d1\u5e03\u4f1a\uff0c\u5ba3\u5e03\u5185\u5730\u5b9a\u6863 2019\u5e747\u670819\u65e5 \u3002', 'spo_list': [('2019\u5e747\u670819\u65e5', '\u5b9a\u6863', '\u5f90\u6d69\u5cf0\u5bfc\u6f14\u65b0\u6b66\u4fa0\u5de8\u5236\u300a\u5200\u80cc\u85cf\u8eab\u300b')]}, {'text': '\u51fb\u6740\u6bd41-17\uff0c top\u4e8c\u961f \u540a\u6253 edg\u4e00\u961f \uff0c\u90a3\u4e2a\u6570\u5b57\u5237\u5c4f\u4e86', 'spo_list': [('top\u4e8c\u961f', '\u540a\u6253', 'edg\u4e00\u961f')]}, {'text': '\u8db3\u7403\uff5c\u7f8e\u6d32\u676f\uff1a \u5df4\u897f \u6dd8\u6c70 \u5df4\u62c9\u572d \u664b\u7ea7\u56db\u5f3a', 'spo_list': [('\u5df4\u897f', '\u6dd8\u6c70', '\u5df4\u62c9\u572d')]}, {'text': '\u4e13\u4e1a\u540d\u79f0 \u6750\u6599\u6210\u578b\u4e0e\u63a7\u5236\u6280\u672f \u4fee\u4e1a\u5e74\u9650 \u4e09\u5e74 \u5b66\u4e1a\u5c42\u6b21\u672c\u79d1\u6216\u4e13\u79d1\u5b66\u4e1a\u6027\u8d28\u666e\u901a\u9ad8\u7b49\u6559\u80b2\u5168\u65e5\u5236\u4e13\u79d1\u5b66\u79d1\u95e8\u7c7b\u5236\u9020\u7c7b\u57f9\u517b\u76ee\u6807\u6838\u5fc3\u80fd\u529b\u6750\u6599\u6210\u578b\u5de5\u827a\u7684\u7f16\u5236\u3001\u5b9e\u65bd\u53ca\u8d28\u91cf\u63a7\u5236', 'spo_list': [('\u6750\u6599\u6210\u578b\u4e0e\u63a7\u5236\u6280\u672f', '\u4fee\u4e1a\u5e74\u9650', '\u4e09\u5e74')]}, {'text': '\u5317\u4eac\u65f6\u95f49\u670825\u65e5\uff0c\u65e5\u672c\u5929\u7687\u676f\u7ed3\u675f\u4e86\u7b2c\u56db\u8f6e\u7684\u4e00\u573a\u6bd4\u8d5b\uff0c\u6dd8\u6c70\u4e86 \u4e0a\u6d77\u4e0a\u6e2f \u7684 \u6d66\u548c\u7ea2\u94bb \u8868\u73b0\u8ba9\u4eba\u5927\u8dcc\u773c\u955c\uff0c\u7adf\u71360-2\u4e0d\u654c\u4f4e\u7ea7\u522b\u7403\u961f\uff08\u4e1a\u4f59\u8054\u8d5b\uff09fc\u672c\u7530\uff0c\u60e8\u906d\u6dd8\u6c70\u3002\u8fd9\u6837\u4e00\u6765\uff0c \u6d66\u548c\u7ea2\u94bb 3\u7ebf\u5d29\u76d8\uff0c\u8054\u8d5b\u8981\u4e3a\u4fdd\u7ea7\u800c\u6218\uff0c\u5929\u7687\u676f\u3001j\u8054\u8d5b\u676f\u5168\u90e8\u51fa\u5c40\uff01\u53ea\u6709\u4e9a\u51a0\u4e86\u3002\u503c\u5f97\u4e00\u63d0\u7684\u662f\uff0c \u6d66\u548c\u7ea2\u94bb \u8fd8\u4e22\u6389\u4e86\u4e0b\u8d5b\u5b63\u7684\u4e9a\u51a0\u8d44\u683c\u3002', 'spo_list': [('\u6d66\u548c\u7ea2\u94bb', '\u4e0d\u654c', '\u4e0a\u6d77\u4e0a\u6e2f')]}, {'text': '\u53e6\u636e\u7f8e\u56fd\u6709\u7ebf\u7535\u89c6\u65b0\u95fb\u7f51\uff08cnn\uff094\u67082\u65e5\u62a5\u9053\uff0c\u5728\u4e0a\u5468\uff0c \u6709\u7ea6660\u4e07\u7f8e\u56fd\u4eba \u7533\u8bf7\u4e86 \u521d\u6b21\u5931\u4e1a\u6551\u6d4e \uff0c\u521b\u4e0b\u5386\u53f2\u65b0\u9ad8', 'spo_list': [('\u6709\u7ea6660\u4e07\u7f8e\u56fd\u4eba', '\u7533\u8bf7', '\u521d\u6b21\u5931\u4e1a\u6551\u6d4e')]}, {'text': ' \u9999\u6e2f\u8b66\u65b9 \uff1a \u4e0a\u5468\u672b \u62d8\u6355159\u4eba6\u6708\u6765\u5171\u62d8\u63551117\u4eba', 'spo_list': [('\u4e0a\u5468\u672b', '\u62d8\u6355', '\u9999\u6e2f\u8b66\u65b9')]}, {'text': '\u82f1\u96c4\u8054\u76df\uff1a\u5728 \u4eca\u65e5 \u7ed3\u675f\u7684 s9\u5168\u7403\u603b\u51b3\u8d5b\u5c0f\u7ec4\u8d5b \u7b2c\u4e09\u65e5\u6700\u540e\u4e00\u573ag2\u5bf9\u9635c9\u7684\u6bd4\u8d5b\u4e2d\uff0cg2\u4e0a\u91ce\u5728\u524d\u671f\u9891\u7e41\u906d\u5230\u51fb\u6740\uff0c\u4e00\u5ea6\u4fdd\u6301\u8d1f\u6218\u7ee9\uff0c\u4e0d\u8fc7g2\u7684\u4e0b\u4e2d\u53d1\u6325\u51fa\u8272\uff0c\u5e26\u9886\u961f\u4f0d\u62ff\u5230\u4e86\u80dc\u5229\uff0c\u8fd9\u4e00\u573a\u662fs9\u76ee\u524d\u4e3a\u6b62\u7528\u65f6\u6700\u77ed\u7684\u6bd4\u8d5b\u3002', 'spo_list': [('\u4eca\u65e5', '\u80dc\u5229', 's9\u5168\u7403\u603b\u51b3\u8d5b\u5c0f\u7ec4\u8d5b')]}, {'text': ' \u70df\u53f0\u5eb7\u68ee\u6a61\u5851\u5236\u54c1\u6709\u9650\u516c\u53f8 \u7cfb\u4e2d\u5916\u5408\u8d44\u4f01\u4e1a\uff0c\u4f4d\u4e8e\u5c71\u4e1c\u7701\u70df\u53f0\u5e02\u798f\u5c71\u9ad8\u65b0\u6280\u672f\u4ea7\u4e1a\u533a\u9e3f\u798f\u885792\u53f7\uff0c\u5360\u5730\u9762\u79ef 30000\u5e73\u65b9\u7c73 ', 'spo_list': [('\u70df\u53f0\u5eb7\u68ee\u6a61\u5851\u5236\u54c1\u6709\u9650\u516c\u53f8', '\u5360\u5730\u9762\u79ef', '30000\u5e73\u65b9\u7c73')]}, {'text': ' \u4eca\u65e5 \u8d70\u52bf\uff1a \u6c47\u6e90\u901a\u4fe1  \u4eca\u65e5 \u5f3a\u52bf\u5c01\u6da8\u505c\u677f\uff0c\u8be5\u80a1\u8fd1\u4e00\u5e74\u6da8\u505c15\u6b21\u3002', 'spo_list': [('\u4eca\u65e5', '\u6da8\u505c\u677f', '\u6c47\u6e90\u901a\u4fe1')]}, {'text': '\u5728\u8fd9\u671f\u95f4\uff0c\u6c5f\u9752\u8bf4\u6bdb\u4e3b\u5e2d\u90a3\u8fb9\u544a\u8bc9\u5979\uff0c\u8bf4\u662f\u4e3b\u5e2d\u7684\u610f\u89c1\uff0c \u79d8\u4e66 \u4e0d\u518d\u63a5\u53d7 \u6211\u4eec\u7684\u6750\u6599 ', 'spo_list': [('\u79d8\u4e66', '\u4e0d\u518d\u63a5\u53d7', '\u6211\u4eec\u7684\u6750\u6599')]}, {'text': '1\u5e7403\u670820\u65e5\uff0c  \u51af\u7fe0\u73ca  \u5df2\u751f\u4ea7\uff0c  \u82cf\u6c38\u5eb7  \u4e00\u76f4\u966a\u5728\u5f85\u4ea7\u59bb\u5b50\u8eab\u65c1', 'spo_list': [('\u51af\u7fe0\u73ca', '\u4e08\u592b', '\u82cf\u6c38\u5eb7'), ('\u82cf\u6c38\u5eb7', '\u59bb\u5b50', '\u51af\u7fe0\u73ca')]}, {'text': '2\u670815\u65e5 \u56fd\u52a1\u59d4\u5458\u517c\u5916\u957f\u738b\u6bc5 \u5728\u6155\u5c3c\u9ed1\u5b89\u5168\u4f1a\u8bae\u53d1\u8868 \u6f14\u8bb2 \uff0c\u4ecb\u7ecd\u4e2d\u56fd\u4e3a\u6297\u51fb\u65b0\u51a0\u80ba\u708e\u75ab\u60c5\u91c7\u53d6\u7684\u679c\u65ad\u63aa\u65bd\u548c\u53d6\u5f97\u7684\u660e\u663e\u6210\u6548', 'spo_list': [('\u56fd\u52a1\u59d4\u5458\u517c\u5916\u957f\u738b\u6bc5', '\u53d1\u8868', '\u6f14\u8bb2')]}, {'text': ' \u6c7d\u6cb9  \u6bcf\u5428\u964d120\u5143 \u56fd\u5185\u6210\u54c1\u6cb9\u4ef76\u6708\u201c\u4e8c\u8fde\u964d\u201d', 'spo_list': [('\u6c7d\u6cb9', '\u964d', '\u6bcf\u5428\u964d120\u5143')]}, {'text': ' \u957f\u6625\u51c0\u6708\u6f6d\u6ed1\u96ea\u573a \u4f4d\u4e8e\u957f\u6625\u5e02\u4e1c\u5357\u4fa7\u51c0\u6708\u6f6d\u56fd\u5bb6\u98ce\u666f\u540d\u80dc\u533a\u5185\uff0c\u533a\u57df\u9762\u79ef 18\u5e73\u65b9\u516c\u91cc \uff0c\u4e09\u9762\u73af\u6797\uff0c\u4e00\u9762\u4e34\u6c34\uff0c\u5177\u6709\u5f97\u5929\u72ec\u539a\u7684\u533a\u4f4d\u4f18\u52bf\u548c\u65c5\u6e38\u8d44\u6e90', 'spo_list': [('\u957f\u6625\u51c0\u6708\u6f6d\u6ed1\u96ea\u573a', '\u9762\u79ef', '18\u5e73\u65b9\u516c\u91cc')]}, {'text': ' 10\u670810\u65e5 \uff0c \u6c5f\u82cf\u65e0\u9521\u9ad8\u67b6\u6865 \u4fa7\u7ffb\u4e8b\u6545\u81f43\u4eba\u6b7b\u4ea1\uff0c2\u4eba\u53d7\u4f24\u3002\u7ecf\u4e8b\u6545\u8c03\u67e5\u7ec4\u521d\u6b65\u5206\u6790\uff0c\u4e0a\u8de8\u6865\u4fa7\u7ffb\u7cfb\u8fd0\u8f93\u8f66\u8f86\u8d85\u8f7d\u6240\u81f4\uff0c\u76ee\u524d\u4e8b\u6545\u540e\u7eed\u5de5\u4f5c\u8fd8\u5728\u8fdb\u884c\u4e2d\u3002', 'spo_list': [('10\u670810\u65e5', '\u4fa7\u7ffb', '\u6c5f\u82cf\u65e0\u9521\u9ad8\u67b6\u6865')]}, {'text': '\u6587\u7ae0\u6307\u51fa\uff0c\u7f8e\u56fd\u201c\u7f3a\u4e4f\u5f3a\u6709\u529b\u7684\u5168\u56fd\u6027\u534f\u8c03\u201d\uff0c\u201c \u653f\u6cbb\u9886\u5bfc\u4eba\u53d1\u51fa\u7684\u4fdd\u5b88\u4fe1\u606f \u201d\u7ed9\u6297\u75ab\u5e26\u6765 \u56f0\u6270 ', 'spo_list': [('\u653f\u6cbb\u9886\u5bfc\u4eba\u53d1\u51fa\u7684\u4fdd\u5b88\u4fe1\u606f', '\u5e26\u6765', '\u56f0\u6270')]}, {'text': '\u5982\u679c\u624b\u5934\u6bd4\u8f83\u7d27\u5f20\u7684\u8bdd\uff0c\u5927\u5bb6\u9996\u5148\u60f3\u5230\u7684\u5c31\u662f\u8d37\u6b3e\u6216\u8005\u662f\u627e\u8eab\u8fb9\u7684\u4eb2\u670b\u597d\u53cb\u501f\uff0c\u53ef\u662f\u5728\u8fd9\u91cc\uff0c\u5c0f\u7f16\u8981\u7ed9\u6240\u6709\u7684\u5e02\u6c11\u670b\u53cb\u90fd\u63d0\u4e2a\u9192\u4e86\uff0c\u60f3\u8981\u8d37\u6b3e\uff0c\u6700\u597d\u662f\u53bb\u6b63\u89c4\u7684\u94f6\u884c\u6216\u8005\u662f\u673a\u6784\u501f\u8d37\uff0c\u5343\u4e07\u4e0d\u8981\u76f8\u4fe1\u7f51\u7edc\u6216\u8005\u662f\u7535\u8bdd\u91cc\u7684\u91d1\u878d\u501f\u8d37\uff0c\u4ee5\u9632\u4e0a\u5f53\u53d7\u9a97\uff0c\u8fd9\u4e0d\uff0c\u8fd1\u65e5 \u676d\u5dde\u8427\u5c71\u8b66\u65b9 \u5c31\u6253\u6389\u4e86\u4e00\u4e2a\u7535\u4fe1\u8bc8\u9a97\u56e2\u4f19\uff0c\u6293\u83b7\u4e86 40\u540d\u72af\u7f6a\u5acc\u7591\u4eba \uff0c\u5176\u4e2d\u6709\u4e0d\u5c11\u662f00\u540e\u7684\u5728\u6821\u5b66\u751f\uff0c\u6848\u4ef6\u7684\u8d77\u56e0\u662f\uff0c\u676d\u5dde\u8427\u5c71\u7684\u674e\u5148\u751f\u63a5\u5230\u6240\u8c13\u65e0\u62b5\u62bc\u8d37\u6b3e\u7684\u63a8\u9500\u7535\u8bdd\uff0c\u5148\u4ea4\u4e86\u6240\u8c13\u7684399\u5143\u98ce\u63a7\u7ecf\uff0c2000\u5143\u7684\u624b\u7eed\u8d39\uff0c\u7ed3\u679c\u8d37\u6b3e\u6ca1\u62ff\u5230\uff0c\u8fd8\u88ab\u4eba\u5bb6\u62c9\u9ed1\u4e86\uff0c\u7a76\u7adf\u662f\u600e\u4e48\u4e00\u56de\u4e8b\u5462\uff1f\u6211\u4eec\u4e00\u8d77\u6765\u770b\u770b\u3002', 'spo_list': [('\u676d\u5dde\u8427\u5c71\u8b66\u65b9', '\u6293\u83b7', '40\u540d\u72af\u7f6a\u5acc\u7591\u4eba')]}, {'text': ' 6\u670826\u65e5\u65e97\u70b930\u5206\u5de6\u53f3 \uff0c \u9ed1\u9f99\u6c5f\u7701\u5927\u5e86\u5e02 \u91c7\u6cb9\u4e03\u5382\u7684\u901a\u52e4\u8f66\u53d1\u751f\u4e8b\u6545\uff0c\u591a\u4eba\u53d7\u4f24\u660f\u8ff7\uff0c\u5177\u4f53\u60c5\u51b5\u8b66\u65b9\u6b63\u5728\u8c03\u67e5\u4e2d\u3002', 'spo_list': [('6\u670826\u65e5\u65e97\u70b930\u5206\u5de6\u53f3', '\u4e8b\u6545', '\u9ed1\u9f99\u6c5f\u7701\u5927\u5e86\u5e02')]}, {'text': ' \u731b\u9f99 \u8d62 \u96c4\u9e7f \uff0c\u83dc\u9e1f\u6559\u7ec3\u7eb3\u65af\u4e5f\u7b97\u5927\u529f\u81e3\uff0c\u4ed6\u7684\u53d8\u9635\u8ba9\u5b57\u6bcd\u54e5\u5f88\u96be\u53d7', 'spo_list': [('\u731b\u9f99', '\u8d62', '\u96c4\u9e7f')]}, {'text': '\u7ed3\u53d1\u4e4b\u59bb\u90d1\u5b89\u5a1c\u53bb\u4e16\u540e\uff0c1993\u5e74\uff0c80\u9ad8\u9f84\u7684  \u51af\u4ea6\u4ee3  \u4e0e\u5e74\u8fd170\u7684\u8457\u540d\u6f14\u5458  \u9ec4\u5b97\u82f1  \uff08\u5c0f\u540d\u5c0f\u59b9\uff09\u7ed3\u4e3a\u4f09\u4fea', 'spo_list': [('\u51af\u4ea6\u4ee3', '\u59bb\u5b50', '\u9ec4\u5b97\u82f1'), ('\u9ec4\u5b97\u82f1', '\u4e08\u592b', '\u51af\u4ea6\u4ee3')]}, {'text': '16\u65e5\uff0c \u90ed\u7ef4\u56fd\u7b49\u4eba \u505a\u51fa \u5c3c\u5c14-\u4f0d\u5fb7\u7cfb\u9152\u540e\u731d\u6b7b\u7684\u7ed3\u8bba \uff0c\u738b\u7acb\u519b\u672a\u63d0\u51fa\u5f02\u8bae', 'spo_list': [('\u90ed\u7ef4\u56fd\u7b49\u4eba', '\u505a\u51fa', '\u5c3c\u5c14-\u4f0d\u5fb7\u7cfb\u9152\u540e\u731d\u6b7b\u7684\u7ed3\u8bba')]}, {'text': ' \u6b66\u6c49\u5e02\u536b\u751f\u5065\u5eb7\u59d4 \u5728\u5b98\u65b9\u7f51\u7ad9\u53d1\u5e03\u300a \u5173\u4e8e\u5f53\u524d\u6211\u5e02\u80ba\u708e\u75ab\u60c5\u7684\u60c5\u51b5\u901a\u62a5 \u300b\uff0c\u53d1\u73b027\u4f8b\u75c5\u4f8b', 'spo_list': [('\u6b66\u6c49\u5e02\u536b\u751f\u5065\u5eb7\u59d4', '\u53d1\u5e03', '\u5173\u4e8e\u5f53\u524d\u6211\u5e02\u80ba\u708e\u75ab\u60c5\u7684\u60c5\u51b5\u901a\u62a5')]}, {'text': ' \u963f\u62c9\u4f2f\u8054\u5408\u914b\u957f\u56fd\u7684\u6c99\u8fe6\u914b\u957f\u56fd\u4e8c\u738b\u5b50\u5361\u5229\u5fb7\uff08sheikkhalid\uff09 \u65e5\u524d\u4f20\u51fa\u5728 \u82f1\u56fd \u8eab\u4ea1\u7684\u6d88\u606f\uff0c7\u67083\u65e5\u9057\u4f53\u5df2\u88ab\u8fd0\u56de\u963f\u62c9\u4f2f\u8054\u5408\u914b\u957f\u56fd\u6c99\u8fe6\u4e3e\u884c\u56fd\u846c\uff0c\u4e3e\u56fd\u4e0a\u4e0b\u5c06\u4e3a\u4ed6\u54c0\u60bc3\u65e5\u3002', 'spo_list': [('\u963f\u62c9\u4f2f\u8054\u5408\u914b\u957f\u56fd\u7684\u6c99\u8fe6\u914b\u957f\u56fd\u4e8c\u738b\u5b50\u5361\u5229\u5fb7\uff08sheikkhalid\uff09', '\u9057\u4f53', '\u82f1\u56fd')]}, {'text': '\u76f4\u64ad\u54279\u670813\u65e5\u8baf\u5728\u521a\u521a\u7ed3\u675f\u7684\u7537\u7bee\u4e16\u754c\u676f\u534a\u51b3\u8d5b\u4e2d\uff0c \u897f\u73ed\u7259 \u5386\u7ecf\u4e24\u4e2a\u52a0\u65f6\u8d5b\u51fb\u8d25\u4e86 \u6fb3\u5927\u5229\u4e9a \u664b\u7ea7\u51b3\u8d5b\u3002', 'spo_list': [('\u897f\u73ed\u7259', '\u51fb\u8d25', '\u6fb3\u5927\u5229\u4e9a')]}, {'text': '\u6c88\u9633\u4e00\u5973\u5b50\u9152\u5e97\u5185\u88ab\u5f3a\u5978\uff0c \u8b66\u65b9 \u901a\u62a5\uff1a \u5acc\u7591\u4eba \u88ab\u5211\u62d8\uff0c\u52ff\u8f7b\u4fe1\u672a\u7ecf\u8bc1\u5b9e\u8eab\u4efd\u4fe1\u606f', 'spo_list': [('\u8b66\u65b9', '\u5211\u62d8', '\u5acc\u7591\u4eba')]}, {'text': ' \u91d1\u9675\u5927\u5b66 \uff08universityofnanking\uff09\uff0c\u7b80\u79f0 \u91d1\u5927 \uff0c\u8bde\u751f\u4e8e1888\u5e74\uff08\u6e05\u5149\u7eea\u5341\u56db\u5e74\uff09\u7684\u7f8e\u56fd\u57fa\u7763\u6559\u4f1a\u7f8e\u4ee5\u7f8e\u4f1a\uff08\u536b\u65af\u7406\u4f1a\uff0cmethodistchurch\uff09\u5728\u5357\u4eac\u521b\u529e\u7684\u6559\u4f1a\u5927\u5b66\uff0c\u540c\u7f8e\u56fd\u5eb7\u5948\u5c14\u5927\u5b66\u4e3a\u59ca\u59b9\u5927\u5b66', 'spo_list': [('\u91d1\u9675\u5927\u5b66', '\u7b80\u79f0', '\u91d1\u5927')]}, {'text': '\u56e0\u5de5\u4f5c\u53d8\u52a8\uff0c \u9ec4\u5d07\u80dc \u8f9e\u53bb \u6021\u7403\u8d44\u6e90 \u8463\u4e8b\u957f\u3001\u8463\u4e8b\u3001\u8463\u4e8b\u4f1a\u6218\u7565\u59d4\u5458\u4f1a\u3001\u63d0\u540d\u59d4\u5458\u4f1a\u53ca\u85aa\u916c\u4e0e\u8003\u6838\u59d4\u5458\u4f1a\u76f8\u5173\u804c\u52a1\u3002', 'spo_list': [('\u9ec4\u5d07\u80dc', '\u8f9e\u53bb', '\u6021\u7403\u8d44\u6e90')]}, {'text': ' \u73ed\u4e3b\u4efb \u5e26\u9886 \u5b66\u751f\u52a0\u6c34 \uff0c \u73ed\u4e3b\u4efb \u6309\u6c34\u5f00\u5173\uff0c\u5b66\u751f\u5927\u95f4\u9694\u6392\u961f\u63a5\u6c34\uff0c\u51cf\u5c11\u95f4\u63a5\u63a5\u89e6', 'spo_list': [('\u73ed\u4e3b\u4efb', '\u5e26\u9886', '\u5b66\u751f\u52a0\u6c34')]}, {'text': '\u5bf9\u4e8e\u8be5\u4e3e\u52a8\uff0c8\u65e5\u665a\uff0c\u521a\u6267\u884c\u5b8c \u6b66\u6c49 \u2014\u4e09\u4e9a\u2014\u67f3\u5dde\u2014 \u6b66\u6c49 \u822a\u7ebf\uff0c\u518d\u6b21\u56de\u5230 \u6b66\u6c49 \u5929\u6cb3\u673a\u573a\u7684\u6bdb\u9716\u544a\u8bc9\u5357\u90fd\u8bb0\u8005\uff0c\u5f53\u65f6\u98de\u673a\u6b63\u5728\u4e0a\u5ba2\uff0c\u4ed6\u671b\u7740\u8dd1\u9053\u9644\u8fd1\u7684\u9001\u884c\u4eba\u7fa4\u975e\u5e38\u611f\u52a8\uff0c\u201c\u56e0\u4e3a \u6b66\u6c49 \u2018\u5c01\u57ce\u2019\u4e86 76\u5929 \uff0c\u8fd9\u671f\u95f4\u67094\u4e07\u540d\u767d\u8863\u5929\u4f7f\u652f\u63f4 \u6b66\u6c49 ', 'spo_list': [('\u6b66\u6c49', '\u5c01\u57ce', '76\u5929')]}, {'text': '11\u5c81\u7684 \u513f\u5b50 \u665a\u4e0a\u53d1\u6d88\u606f\u7ed9\u6b63\u5728\u533b\u9662\u503c\u73ed\u7684\u533b\u751f\u5988\u5988\uff0c\u8fde\u7eed\u53d1\u4e86\u5341\u591a\u6761\u90fd\u6ca1\u6709\u6536\u5230\u56de\u590d\uff0c\u5fc3\u91cc\u975e\u5e38\u96be\u53d7\uff0c\u4ed6\u4e0d\u77e5\u9053\u6b64\u65f6\u5988\u5988\u6b63\u5728\u5fd9\u788c\u7684\u62a2\u6551\u75c5\u4eba\uff0c\u5f53 \u8fd9\u540d\u533b\u751f\u5988\u5988 \u770b\u5230\u6d88\u606f\u65f6\u5df2\u5feb\u534a\u591c12\u70b9\uff0c\u4e8b\u540e\u89c9\u5f97\u5f88\u6127\u759a\uff0c\u53d1\u4e86\u4e00\u6761\u670b\u53cb\u5708\u5411 \u513f\u5b50 \u9053\u6b49\uff0c\u5374\u611f\u52a8\u4e86\u8eab\u8fb9\u5f88\u591a\u4eba\u3002', 'spo_list': [('\u8fd9\u540d\u533b\u751f\u5988\u5988', '\u9053\u6b49', '\u513f\u5b50')]}, {'text': ' \u5218\u5c11\u5947 \u89e3\u51b3 \u201c\u987a\u76f4\u95ee\u9898\u201d \uff0c\u5f97\u5230\u4e2d\u592e\u7684\u5145\u5206\u80af\u5b9a', 'spo_list': [('\u5218\u5c11\u5947', '\u89e3\u51b3', '\u201c\u987a\u76f4\u95ee\u9898\u201d')]}, {'text': '\u7981\u8d5b\u4e00\u5e74\uff01 \u4e2d\u56fd\u8db3\u534f \u516c\u5e03\u5bf9 \u5f20\u9e6d \u7684\u5904\u7f5a\uff0c\u56fd\u8db3\u9886\u961f\u4ea6\u516c\u5f00\u81f4\u6b49', 'spo_list': [('\u4e2d\u56fd\u8db3\u534f', '\u7981\u8d5b', '\u5f20\u9e6d')]}, {'text': '\u4f5c\u4e3a\u7b2c\u4e8c\u5c4a\u56db\u5ddd\u827a\u672f\u8282\u7684\u7cfb\u5217\u6d3b\u52a8\u4e4b\u4e00\uff0c \u56db\u5ddd\u4eba\u827a\u9996\u5c4a\u513f\u7ae5\u821e\u53f0\u827a\u672f\u516c\u76ca\u6f14\u51fa\u5b63 \uff0c\u4e8e 8\u670813\u65e5 \u6b63\u5f0f\u542f\u5e55\u3002', 'spo_list': [('8\u670813\u65e5', '\u542f\u5e55', '\u56db\u5ddd\u4eba\u827a\u9996\u5c4a\u513f\u7ae5\u821e\u53f0\u827a\u672f\u516c\u76ca\u6f14\u51fa\u5b63')]}, {'text': '\u5927\u660c\u884c\u96c6\u56e2(01828)\u53d1