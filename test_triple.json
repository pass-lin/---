\u65b0\u4e0a\u67b6\u53d1\u552e\uff0c \u4e09\u661f \u5ba3\u5e03\u5df2\u4fee\u590dgalaxyfold', 'spo_list': [('9\u6708', '\u4e0a\u67b6', '\u4e09\u661f')]}, {'text': '\u6839\u636e\u963f\u5fb7\u91cc\u5b89-\u6c83\u7eb3\u7f57\u65af\u57fa\u7684\u62a5\u9053\uff0c\u8054\u76df\u6d88\u606f\u4eba\u58eb\u900f\u9732\uff0c \u675c\u5170\u7279 \u5c06\u4ee54\u5e741.64\u4ebf\u7f8e\u5143\u3001\u6b27\u6587\u4ee54\u5e741.64\u4ebf\u7f8e\u5143\u7684\u5408\u540c\u52a0\u76df \u7bee\u7f51 ', 'spo_list': [('\u675c\u5170\u7279', '\u52a0\u76df', '\u7bee\u7f51')]}, {'text': '\u5728 \u671b\u4eac \u5de5\u4f5c\u7684  \u590f\u5973\u58eb  \u6253\u5f00 \u67d0\u751f\u9c9capp \u53d1\u73b0\uff0c1\u670821\u65e5\u81f331\u65e5\u671f\u95f4\uff0c\u95e8\u5e97\u4e0b\u5355\u914d\u9001\u5c06\u6536\u53d66\u5143\u8fd0\u8d39\uff0c\u800c\u5e73\u65f6\u90fd\u662f\u514d\u8d39\u914d\u9001', 'spo_list': [('\u590f\u5973\u58eb', '\u6253\u5f00', '\u67d0\u751f\u9c9capp'), ('\u590f\u5973\u58eb', '\u5730\u70b9', '\u671b\u4eac')]}, {'text': ' 10\u670812\u65e5 \uff0c\u5728 \u6d4e\u9752\u9ad8\u901f\u9752\u5c9b\u65b9\u5411 \uff0c\u4e00\u5927\u8d27\u8f66\u649e\u5230\u8def\u8fb9\u62a4\u680f\uff0c\u53f8\u673a\u88ab\u56f0\u8f66\u5185\uff0c\u610f\u8bc6\u6a21\u7cca\uff0c\u6d88\u9632\u5458\u7d27\u6025\u6551\u52a9\u3002', 'spo_list': [('10\u670812\u65e5', '\u649e\u5230', '\u6d4e\u9752\u9ad8\u901f\u9752\u5c9b\u65b9\u5411')]}, {'text': '\u5a31\u4e50\u5708\u91cc\u660e\u661f\u4eec\u7684\u5a5a\u604b\u60c5\u51b5\u603b\u662f\u5f88\u53d7\u7f51\u53cb\u4eec\u7684\u5173\u6ce8\uff0c \u6587\u7ae0 \u548c \u9a6c\u4f0a\u740d \u79bb\u5a5a\u7684\u6d88\u606f\u516c\u5f00\u4e86\uff0c\u7f51\u53cb\u4eec\u8bae\u8bba\u7eb7\u7eb7\u3002', 'spo_list': [('\u6587\u7ae0', '\u79bb\u5a5a', '\u9a6c\u4f0a\u740d')]}, {'text': ' 2019\u5e7410\u670815\u65e5 \uff0c\u5728\u4f4f\u5b85\u4e2d\u4e0a\u540a\u81ea\u6740\u7684 \u5d14\u96ea\u8389 \u544a\u522b\u8fd9\u4e2a\u4e16\u754c\uff0c\u8fd9\u4f4d\u6b63\u503c25\u5c81\u82b1\u6837\u5e74\u534e\u7684\u59d1\u5a18\u7684\u79bb\u53bb\uff0c\u7ed9\u4e16\u754c\u7559\u4e0b\u4e86\u6c89\u91cd\u4e14\u60b2\u75db\u7684\u4e00\u4e2a\u6559\u8bad\uff0c\u53bb\u4e16\u7684\u540e\u7eed\u6b63\u5728\u4e0d\u65ad\u5c55\u5f00\uff0c\u97e9\u56fd\u8b66\u65b9\u5c06\u5bf9\u5b9e\u4f53\u8fdb\u884c\u68c0\u9a8c\u8c03\u67e5\uff0csm\u4e5f\u58f0\u660e\u9075\u4ece\u9057\u5631\u4e3a\u5979\u7684\u7c89\u4e1d\u51c6\u5907\u8c03\u7814\u573a\u6240\uff0c\u97e9\u56fd\u5a31\u4e50\u5708\u7684\u660e\u661f\u4eec\u66f4\u662f\u5bf9\u5979\u7684\u901d\u53bb\u60cb\u60dc\u4e0d\u5df2\u7b49\u7b49\uff0c\u800c\u4e00\u5207\u7684\u4e00\u5207\u7684\u4e89\u8bae\u90fd\u968f\u7740\u8fd9\u4f4d\u59d1\u5a18\u7684\u901d\u53bb\u800c\u6d88\u6563\u3002', 'spo_list': [('2019\u5e7410\u670815\u65e5', '\u81ea\u6740', '\u5d14\u96ea\u8389')]}, {'text': '\u5728\u7ecf\u8425\u7535\u5546\u5e73\u53f0\u7684\u540c\u65f6\uff0c \u5218\u5b66\u8d85 \u8fd8\u4e3a\u5f53\u5730\u519c\u6237\u57f9\u8bad\uff0c\u5efa\u7acb \u521b\u4e1a\u5b75\u5316\u5668 \uff0c\u548c\u519c\u6237\u4eec\u4e00\u8d77\u201c\u8131\u4f4e\u589e\u6536\u201d', 'spo_list': [('\u5218\u5b66\u8d85', '\u5efa\u7acb', '\u521b\u4e1a\u5b75\u5316\u5668')]}, {'text': '\u674e\u5e72\u6770\u6307\u51fa, \u8fd9\u4e9b\u63aa\u65bd \u786e\u4fdd\u76d1\u6d4b\u6570\u636e\u771f\u5b9e\u51c6\u786e,\u4f7f\u5f97\u76d1\u6d4b\u6570\u636e\u548c\u8001\u767e\u59d3\u7684\u611f\u53d7\u5b8c\u5168\u4e00\u81f4,\u4e5f\u89e3\u51b3\u4e86 \u8fc7\u53bb\u201c\u4e24\u5f20\u76ae\u201d\u7684\u95ee\u9898 ', 'spo_list': [('\u8fd9\u4e9b\u63aa\u65bd', '\u89e3\u51b3', '\u8fc7\u53bb\u201c\u4e24\u5f20\u76ae\u201d\u7684\u95ee\u9898')]}, {'text': ' \u66f9\u4e91\u91d1 \u672c\u6765\u662f\u4e2a\u76f8\u58f0\u6f14\u5458\uff0c\u56e0\u4e0e\u5176\u5e08\u5085\u90ed\u5fb7\u7eb2\u7684\u77db\u76fe\u7ea0\u7eb7\u88ab\u5927\u5bb6\u719f\u77e5\uff0c\u800c\u5728\u524d\u6bb5\u65f6\u95f4\uff0c \u66f9\u4e91\u91d1 \u4e0e\u5176 \u59bb\u5b50 \u79bb\u5a5a\u7684\u4e8b\u60c5\uff0c\u4e5f\u5728\u5a31\u4e50\u5708\u95f9\u5f97\u6cb8\u6cb8\u626c\u626c\u3002', 'spo_list': [('\u66f9\u4e91\u91d1', '\u79bb\u5a5a', '\u59bb\u5b50')]}, {'text': ' \u5580\u62c9\u4e5f\u6728\u52d2\u98ce\u666f\u533a \u4f4d\u4e8e\u65b0\u7586\u5854\u57ce\u5730\u533a\u989d\u654f\u53bf\u57ce\u53bf\u57ce\u4ee5\u4e1c65\u516c\u91cc\u5904\u5580\u62c9\u4e5f\u6728\u52d2\u4e61\uff0c\u666f\u533a\u9762\u79ef\u7ea6 20\u5e73\u65b9\u516c\u91cc \uff0c\u5317\u9760\u76f4\u5170\u7279\u5c71\uff0c\u5357\u90e8\u4e3a\u5929\u7136\u8349\u573a', 'spo_list': [('\u5580\u62c9\u4e5f\u6728\u52d2\u98ce\u666f\u533a', '\u5360\u5730\u9762\u79ef', '20\u5e73\u65b9\u516c\u91cc')]}, {'text': '  \u9648\u79c0\u8fde  \uff08 1845 \uff5e1927\uff09\uff0c\u5b87\u7389\u6c60\uff0c\u7956\u7c4d \u756a\u79ba\u957f\u6e74\u6751 \uff08\u4eca\u5c5e\u5e7f\u5dde\u5e02\uff09\uff0c\u5546\u4eba', 'spo_list': [('\u9648\u79c0\u8fde', '\u51fa\u751f\u65e5\u671f', '1845'), ('\u9648\u79c0\u8fde', '\u7956\u7c4d', '\u756a\u79ba\u957f\u6e74\u6751')]}, {'text': '2. \u7ee7\u53d1\u6027\u6897\u963b\u6027\u5de8\u8f93\u5c3f\u7ba1 \u5e38\u89c1\u4e8e\u795e\u7ecf\u6e90\u6027\u8180\u80f1\u6216\u975e\u795e\u7ecf\u6e90\u6027\u6392\u5c3f\u529f\u80fd\u969c\u788d\u53ca\u4e0b\u5c3f\u8def\u6897\u963b\uff08\u5982\u540e\u5c3f\u9053\u74e3\u819c\uff09\u3002\u5b83\u4eec\u5bfc\u81f4 \u8180\u80f1\u5185\u538b\u589e\u9ad8 \u3001\u8180\u80f1\u58c1\u6216\u8f93\u5c3f\u7ba1\u8fdc\u7aef\u7ea4\u7ef4\u5316\uff0c\u5f62\u6210\u8180\u80f1\u8f93\u5c3f\u7ba1\u8fde\u63a5\u90e8\u7684\u6897\u963b\u3002', 'spo_list': [('\u7ee7\u53d1\u6027\u6897\u963b\u6027\u5de8\u8f93\u5c3f\u7ba1', '\u53d1\u75c5\u673a\u5236', '\u8180\u80f1\u5185\u538b\u589e\u9ad8')]}, {'text': '\u4e34\u9635\u8131\u9003\uff1f \u4e2d\u56fdu18\u56fd\u9752 \u5ba3\u5e03\u9000\u8d5b \u4e2d\u8d6b\u56fd\u5b89\u676f \uff0c\u6276\u4e0d\u8d77\u7684\u963f\u6597\uff01', 'spo_list': [('\u4e2d\u56fdu18\u56fd\u9752', '\u9000\u8d5b', '\u4e2d\u8d6b\u56fd\u5b89\u676f')]}, {'text': '\u6b64\u533a\u5c71\u9ad8\u8303\u56f4\u5e7f\uff0c\u5cf0\u4e1b\u6797\u7acb\uff0c\u53555000\u7c73\u4ee5\u4e0a\u9ad8\u5cf0\u5c31\u8fbe7\u5ea7\uff0c\u5176\u4e2d\u53d1\u80b2\u7740 \u96ea\u5b9d\u9f0e ( 5588\u7c73 )\u3001\u96ea\u680f\u5c71(5440\u7c73)\u548c\u95e8\u6d1e\u5cf0(5058\u7c73)\u4e09\u6761\u73b0\u4ee3\u51b0\u5ddd\uff0c\u4f7f\u6b64\u533a\u57df\u6210\u4e3a\u4e2d\u56fd\u6700\u4e1c\u90e8\u7684\u73b0\u4ee3\u51b0\u5ddd\u4fdd\u5b58\u533a', 'spo_list': [('\u96ea\u5b9d\u9f0e', '\u6d77\u62d4', '5588\u7c73')]}, {'text': '10\u670815\u65e5\uff0c\u8bb0\u8005\u4ece\u6df1\u5733\u5e02\u5e94\u6025\u7ba1\u7406\u5c40\u83b7\u6089\uff0c \u4eca\u5e748\u6708 \uff0c\u5b89\u5168\u751f\u4ea7\u4e3e\u62a5\u6295\u8bc9\u7279\u670d\u7535\u8bdd\"12350\"\u63a5\u5230 \u7fa4\u4f17 \u4e3e\u62a5\uff0c\u6bcf\u665a9\u70b9\u5de6\u53f3\uff0c\u5149\u660e\u533a\u9a6c\u7530\u8857\u9053\u4e00\u5355\u4f4d\u5c31\u4f1a\u63d0\u4f9b\u573a\u5730\u7ed9\u60e0\u5dde\u67d0\u5de5\u5382\u8d29\u5356\u4e59\u7094\u3002\u63a5\u5230\u4e3e\u62a5\u540e\uff0c\u6df1\u5733\u591a\u90e8\u95e8\u8054\u5408\u6267\u6cd5\uff0c\u6700\u7ec8\u67e5\u626313\u53f0\u8fd0\u8f93\u82e5\u5e72\u4e59\u7094\u7b49\u5371\u9669\u5316\u5b66\u54c1\uff0c\u5171\u8ba1\u7f5a\u6b3e56\u4e07\u5143\u3002', 'spo_list': [('\u4eca\u5e748\u6708', '\u4e3e\u62a5', '\u7fa4\u4f17')]}, {'text': ' \u5b59\u7acb\u4eba \u5ba1\u95ee\u4fd8\u864f\uff0c\u53d1\u73b0 \u65e5\u519b\u81f3\u5c11\u4e24\u4e2a\u5927\u961f\u7684\u756a\u53f7 \uff0c\u8ba4\u4e3a\u60c5\u51b5\u4e0d\u5bf9\uff0c\u4e0e\u67cf\u5927\u5435\uff0c\u8981\u6c42\u7acb\u5373\u6d3e\u51fa\u63f4\u519b', 'spo_list': [('\u5b59\u7acb\u4eba', '\u53d1\u73b0', '\u65e5\u519b\u81f3\u5c11\u4e24\u4e2a\u5927\u961f\u7684\u756a\u53f7')]}, {'text': '\u7b80\u4ecb\uff1a \u7b2c\u4e5d\u5c4a\u5317\u4eac\u9999\u5c71\u8bba\u575b 21\u65e5\u4e0a\u5348\u5728 \u5317\u4eac\u56fd\u9645\u4f1a\u8bae\u4e2d\u5fc3 \u5f00\u5e55\u3002\u5317\u4eac\u9999\u5c71\u8bba\u575b\u4e8e2006\u5e74\u521b\u529e\u3002\u672c\u5c4a\u8bba\u575b\u4ee5\u201c\u7ef4\u62a4\u56fd\u9645\u79e9\u5e8f\uff0c\u5171\u7b51\u4e9a\u592a\u548c\u5e73\u201d\u4e3a\u4e3b\u9898\uff0c\u670976\u4e2a\u56fd\u5bb6\u548c\u56fd\u9645\u7ec4\u7ec7\u5b98\u65b9\u4ee3\u8868\u56e2\uff0c\u4ee5\u53ca53\u4e2a\u56fd\u5bb6\u7684\u4e13\u5bb6\u5b66\u8005\uff0c\u51711300\u591a\u4eba\u53c2\u4f1a\u3002', 'spo_list': [('\u7b2c\u4e5d\u5c4a\u5317\u4eac\u9999\u5c71\u8bba\u575b', '\u5f00\u5e55', '\u5317\u4eac\u56fd\u9645\u4f1a\u8bae\u4e2d\u5fc3')]}, {'text': '\u4e3a\u4e86\u5411\u4e2d\u56fd\u6fb3\u95e8\u8db3\u603b\u5ba3\u5e03\u653e\u5f03\u4e16\u9884\u8d5b\u7b2c\u4e00\u8f6e\u6b21\u56de\u5408\u7684\u51b3\u5b9a\u4f5c\u51fa\u6297\u8bae\uff0c\u4eca\u65e5\u4e0b\u5348\u4e24\u70b9\u5728\u83b2\u5cf0\u7403\u573a\u8fdb\u884c\u7684\u4e00\u573a\u6fb3\u95e8\u7532\u7ec4\u8db3\u603b\u676f\u6bd4\u8d5b\u4e2d\uff0c \u52a0\u4e49 \u548c \u6a58\u4e95 \u6052\u52bf\u4e24\u961f\u5355\u573a\u6253\u5165\u4e86\u60ca\u4eba\u768439\u7403\uff0c\u6700\u7ec8 \u52a0\u4e49 21-18 \u6a58\u4e95 \u53d6\u5f97\u4e86\u80dc\u5229\u3002', 'spo_list': [('\u52a0\u4e49', '\u80dc\u5229', '\u6a58\u4e95')]}, {'text': ' \u79d1\u5b66\u5bb6 \u5728\u653f\u6cbb\u4e0a\u53cd\u5bf9 \u6211\u4eec\u515a\u3001\u653f\u5e9c \uff0c\u662f\u6ca1\u6709\u8fd9\u6837\u4e25\u91cd\u7684\uff0c\u4e3b\u8981\u662f\u7262\u9a9a\uff0c\u56e0\u4e3a\u6761\u4ef6\u4e0d\u591f\uff0c\u6ca1\u6709\u673a\u4f1a\uff0c\u5de5\u4f5c\u6ca1\u6709\u6210\u679c\u7b49\uff0c\u8fd9\u4ecd\u8fd8\u7275\u6d89\u5230\u6211\u4eec\u7684\u9886\u5bfc', 'spo_list': [('\u79d1\u5b66\u5bb6', '\u53cd\u5bf9', '\u6211\u4eec\u515a\u3001\u653f\u5e9c')]}, {'text': ' rng \u4e0e skt \u5dc5\u5cf0\u5bf9\u51b3\uff0c skt \u5947\u62db\u5236\u80dc\uff0cclid\u201c\u9690\u85cfmvp\u201d', 'spo_list': [('skt', '\u5236\u80dc', 'rng')]}, {'text': '\u4e2d\u65b0\u793e\u6bd4\u4ec0\u51ef\u514b 5\u670821\u65e5 \u7535(\u8bb0\u8005\u6587\u9f99\u6770) 5\u670821\u65e5 \uff0c \u4e2d\u56fd\u56fd\u52a1\u59d4\u5458\u517c\u5916\u4ea4\u90e8\u957f\u738b\u6bc5 \u5728\u6bd4\u4ec0\u51ef\u514b\u540c\u5409\u5c14\u5409\u65af\u65af\u5766\u5916\u957f\u827e\u8fbe\u5c14\u522b\u79d1\u592b\u4e3e\u884c\u4f1a\u8c08\u3002', 'spo_list': [('5\u670821\u65e5', '\u4f1a\u8c08', '\u4e2d\u56fd\u56fd\u52a1\u59d4\u5458\u517c\u5916\u4ea4\u90e8\u957f\u738b\u6bc5')]}, {'text': '\u56fe\u96c6\u201c\u5341\u56db\u4e94\u201d\u7535\u7f51\u89c4\u5212\u7f16\u5236\u8ba1\u5212\u5e74\u5185\u5b8c\u6210\u201c\u65b0\u57fa\u5efa\u201d\u52a0\u901f\u8dd1\u7279\u9ad8\u538b\u8fce\u91cd\u91d1\u5e03\u5c40\u300a\u7ecf\u6d4e\u53c2\u8003\u62a5\u300b\u8bb0\u8005\u4e86\u89e3\u5230\uff0c \u56fd\u5bb6\u7535\u7f51\u516c\u53f8 \uff08\u4e0b\u79f0\u201c\u56fd\u5bb6\u7535\u7f51\u201d\uff09\u6b63\u5728\u52a0\u7d27\u7ec4\u7ec7\u5f00\u5c55\u201c \u5341\u56db\u4e94\u201d\u7535\u7f51\u89c4\u5212\u7814\u7a76\u5de5\u4f5c \uff0c\u8ba1\u5212\u5e74\u5185\u5b8c\u6210\u89c4\u5212\u62a5\u544a\u7f16\u5236\uff0c\u4e0a\u62a5\u56fd\u5bb6\u80fd\u6e90\u5c40', 'spo_list': [('\u56fd\u5bb6\u7535\u7f51\u516c\u53f8', '\u5f00\u5c55', '\u5341\u56db\u4e94\u201d\u7535\u7f51\u89c4\u5212\u7814\u7a76\u5de5\u4f5c')]}, {'text': '\u8fd9\u4e00\u5929\uff0c \u201c\u695a\u6cb3\u6c49\u8857\u201d \u5f00\u59cb\u6062\u590d \u8425\u4e1a ', 'spo_list': [('\u201c\u695a\u6cb3\u6c49\u8857\u201d', '\u6062\u590d', '\u8425\u4e1a')]}, {'text': ' 8\u670814\u65e5\u665a\u4e0a8\u70b9 \uff0c v5\u7535\u5b50\u7ade\u6280\u4ff1\u4e50\u90e8 \u51c6\u65f6\u53d1\u5e03\u4e86\u5b98\u65b9\u58f0\u660e\uff0c\u5bf9\u4e8ev5\u6559\u7ec3hanyi\u7684\u8fc7\u6fc0\u8a00\u8bba\u8fdb\u884c\u4e86\u9053\u6b49\uff0c\u9887\u6709\u4e9b\u606f\u4e8b\u5b81\u4eba\u7684\u610f\u601d\u3002hanyi\u4e5f\u5220\u9664\u4e86\u4e4b\u524d\u7684\u8fc7\u6fc0\u53d1\u8a00\uff0c\u5e76\u57288\u70b904\u5206\u53d1\u5e03\u4e86\u9053\u6b49\u5fae\u535a\uff0c\u5728\u627f\u8ba4\u81ea\u5df1\u9519\u8bef\u7684\u540c\u65f6\uff0c\u771f\u8bda\u5730\u56de\u5e94\u4e86\u7f51\u53cb\u4eec\u53e3\u8bdb\u7b14\u4f10\u7684bp\u95ee\u9898\uff0c\u8fd8\u5728\u8bc4\u8bba\u533a\u9884\u544a\u81ea\u5df1\u4f1a\u5728\u4f11\u8d5b\u671f\u53d1\u4e00\u7bc7\u6587\u7ae0\u7ed9\u5927\u5bb6\u89e3\u91ca\u4ed6\u7684bp\u3002', 'spo_list': [('8\u670814\u65e5\u665a\u4e0a8\u70b9', '\u9053\u6b49', 'v5\u7535\u5b50\u7ade\u6280\u4ff1\u4e50\u90e8')]}, {'text': ' 10\u670817\u65e5 \u6d88\u606f\uff0c \u4e2d\u56fd\u94f6\u4fdd\u76d1\u4f1a\u4e3d\u6c34\u76d1\u7ba1\u5206\u5c40 \u53d1\u5e03\u884c\u653f\u5904\u7f5a\u4fe1\u606f\u516c\u5f00\u8868\u663e\u793a\uff0c\u4e2d\u534e\u8054\u5408\u8d22\u4ea7\u4fdd\u9669\u80a1\u4efd\u6709\u9650\u516c\u53f8\u4e3d\u6c34\u4e2d\u5fc3\u652f\u516c\u53f8\u56e0\u672a\u6309\u7167\u89c4\u5b9a\u4f7f\u7528\u7ecf\u6279\u51c6\u6216\u5907\u6848\u7684\u4fdd\u9669\u6761\u6b3e\u3001\u4fdd\u9669\u8d39\u7387\uff0c\u4ee5\u516c\u4f17\u8d23\u4efb\u9669\u627f\u4fdd\u673a\u52a8\u8f66\u7b2c\u4e09\u8005\u8d23\u4efb\u9669\uff0c\u88ab\u7f5a\u6b3e25\u4e07\u5143\u3002', 'spo_list': [('10\u670817\u65e5', '\u7f5a\u6b3e', '\u4e2d\u56fd\u94f6\u4fdd\u76d1\u4f1a\u4e3d\u6c34\u76d1\u7ba1\u5206\u5c40')]}, {'text': '\u6b64\u524d\u7684 8\u67085\u65e5 \uff0c \u8303\u5b81 \u7a81\u7136\u5ba3\u5e03\u9000\u4efb\u6c47\u4e30\u63a7\u80a1\u884c\u653f\u603b\u88c1\u4e00\u804c\u3002', 'spo_list': [('8\u67085\u65e5', '\u9000\u4efb', '\u8303\u5b81')]}, {'text': ' \u6d4e\u5b81\u5e02\u5e02\u573a\u76d1\u7ba1\u5c40 \u6574\u6cbb\u96f6\u552e\u836f\u5e97\u4e0d\u51ed\u5904\u65b9\u9500\u552e\u5904\u65b9\u836f\u7b49\u8fdd\u6cd5\u8fdd\u89c4\u884c\u4e3a\u3002\u622a\u81f3\u76ee\u524d\uff0c\u5168\u5e02\u5df2\u68c0\u67e5\u96f6\u552e\u836f\u5e971105\u5bb6\uff0c\u8b66\u544a155\u5bb6\uff0c\u9650\u671f\u6574\u6539215\u5bb6\uff0c\u6536\u56de\u6216\u64a4\u9500\u836f\u54c1gsp\u8bc1\u4e6612\u5bb6\uff0c\u7acb\u6848\u67e5\u590436\u5bb6\uff0c\u7f5a\u6b3e 13.23\u4e07\u5143 \u3002', 'spo_list': [('13.23\u4e07\u5143', '\u7f5a\u6b3e', '\u6d4e\u5b81\u5e02\u5e02\u573a\u76d1\u7ba1\u5c40')]}, {'text': ' \u4e54\u51a0\u534e \u662f\u4e2d\u56fd\u8457\u540d\u5916\u4ea4\u5bb6\uff0c1974\u5e7411\u6708\u81f31976\u5e7412\u6708\u51fa\u4efb \u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5916\u4ea4\u90e8\u90e8\u957f ', 'spo_list': [('\u4e54\u51a0\u534e', '\u51fa\u4efb', '\u4e2d\u534e\u4eba\u6c11\u5171\u548c\u56fd\u5916\u4ea4\u90e8\u90e8\u957f')]}, {'text': ' \u5b69\u5b50\u5760\u4ea1\u5bb6\u957f \u8d77\u8bc9 \u623f\u4e1c \u6848\u56fd\u5185\u5df2\u6709\u591a\u8d77\u6cd5\u9662\u4e3a\u4ec0\u4e48\u5224 \u623f\u4e1c \u627f\u62c5\u6b21\u8981\u8d23\u4efb\uff1f', 'spo_list': [('\u5b69\u5b50\u5760\u4ea1\u5bb6\u957f', '\u8d77\u8bc9', '\u623f\u4e1c')]}, {'text': '\u622a\u81f33\u67081\u65e5\uff0c\u6b66\u6c49\u5e02\u6148\u5584\u603b\u4f1a\u5171\u6536\u5230\u793e\u4f1a\u6350\u6b3e40.4432\u4ebf\u5143\uff1b \u6b66\u6c49\u7ea2\u5341\u5b57\u4f1a \u7d2f\u8ba1\u63a5\u53d7 \u793e\u4f1a\u6350\u8d60\u8d44\u91d115.0852\u4ebf\u5143 \uff0c\u8fd8\u63a5\u6536\u4e86\u5927\u91cf\u5404\u7c7b\u9632\u63a7\u6025\u9700\u7269\u8d44', 'spo_list': [('\u6b66\u6c49\u7ea2\u5341\u5b57\u4f1a', '\u63a5\u53d7', '\u793e\u4f1a\u6350\u8d60\u8d44\u91d115.0852\u4ebf\u5143')]}, {'text': ' \u5317\u4eac\u65f6\u95f4\u6628\u5929\u4e0a\u5348 \uff0c\u4f11\u65af\u987f\u706b\u7bad\u961f\u4e0e\u4fc4\u514b\u62c9\u8377\u9a6c\u96f7\u9706\u961f\u5b8c\u6210\u4e86\u4e00\u7b14\u9707\u60ca\u8054\u76df\u7684\u4ea4\u6613\uff0c\u706b\u7bad\u961f\u9001\u51fa\u4e86 \u540e\u536b\u7403\u5458\u514b\u91cc\u65af-\u4fdd\u7f57 \u30012024\u5e74\u548c2026\u5e74\u7684\u9996\u8f6e\u7b7e\uff0c\u4ee5\u53ca2021\u5e74\u548c2025\u5e74\u9996\u8f6e\u7b7e\u4e92\u6362\u6743\uff0c\u4ece\u96f7\u9706\u961f\u5f97\u5230\u62c9\u585e\u5c14-\u5a01\u65af\u5e03\u9c81\u514b\u3002\u968f\u540e\uff0c\u706b\u7bad\u961f\u603b\u7ecf\u7406\u8fbe\u96f7\u5c14-\u83ab\u96f7\u5728\u62c9\u65af\u7ef4\u52a0\u65af\u63a5\u53d7\u4e86\u91c7\u8bbf\uff0c\u8c08\u5230\u4e86\u4ea4\u6613\u4e2d\u7684\u4e00\u4e9b\u7ec6\u8282\u3002', 'spo_list': [('\u5317\u4eac\u65f6\u95f4\u6628\u5929\u4e0a\u5348', '\u9001\u51fa\u4e86', '\u540e\u536b\u7403\u5458\u514b\u91cc\u65af-\u4fdd\u7f57')]}, {'text': ' \u52a8\u89c6\u66b4\u96ea \u5927\u88c1\u5458 \u8fd1\u5343\u4eba \uff01\u4e24\u5927\u6e38\u620f\u65b9\u8054\u624b\u63a7\u8bc9\u201c\u795e\u4ed9\u6253\u67b6\u201d\u73a9\u5bb6\u906d\u6b83', 'spo_list': [('\u52a8\u89c6\u66b4\u96ea', '\u88c1\u5458', '\u8fd1\u5343\u4eba')]}, {'text': ' 56\u5c81 \u7684\u4e2d\u5fc3\u5c0f\u5b66\u526f\u6821\u957f\u88a