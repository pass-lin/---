u8f93\u4e00\u5c40\u7684\u60c5\u51b5\u4e0b\uff0c\u968f\u540e\u8fde\u4e0b\u4e09\u5c40\uff0c\u6700\u7ec8\u4ee53-1\u6210\u529f\u6218\u80dc\u4e86 rng \uff0c\u62ff\u5230\u4e86lpl\u590f\u5b63\u603b\u51a0\u519b\uff0c\u8ba9\u6211\u4eec\u5728\u8fd9\u4e2a\u590f\u5929\u518d\u6b21\u89c1\u8bc1\u4e86\u65b0\u738b\u767b\u57fa\uff0c\u606d\u559c\u5c0f\u51e4\u51f0\u606d\u559c fpx \uff01', 'spo_list': [('fpx', '\u6218\u80dc', 'rng')]}, {'text': '5\u670824\u65e5\uff0c\u90af\u90f8\u5e02\u5cf0\u5cf0\u77ff\u533a\u4eba\u6c11\u6cd5\u9662\u5bf9\u6cb3\u5317\u90af\u90f8\u5e02\u516c\u5b89\u5c40\u6c38\u5e74\u533a\u5206\u5c40\u539f\u515a\u59d4\u59d4\u5458\u3001\u526f\u5c40\u957f\u8d75\u67d0\u5cf0\u5f87\u79c1\u6789\u6cd5\u3001\u53d7\u8d3f\u4e00\u6848\u8fdb\u884c\u516c\u5f00\u5ba3\u5224\u3002 \u88ab\u544a\u4eba\u8d75\u67d0\u5cf0 \u72af\u5f87\u79c1\u6789\u6cd5\u7f6a\u3001\u53d7\u8d3f\u7f6a\uff0c\u51b3\u5b9a\u6267\u884c\u6709\u671f\u5f92\u5211 \u4e94\u5e74 \u3002 \u88ab\u544a\u4eba\u8d75\u67d0\u5cf0 \u5df2\u9000\u7f34\u7684\u8fdd\u6cd5\u6240\u5f97\u4e88\u4ee5\u6ca1\u6536\uff0c\u4e0a\u7f34\u56fd\u5e93\u3002', 'spo_list': [('\u88ab\u544a\u4eba\u8d75\u67d0\u5cf0', '\u6709\u671f\u5f92\u5211', '\u4e94\u5e74')]}, {'text': '\u5728\u6e56\u5317\u5a92\u4f53\u7b14\u4e0b\uff0c \u5b59\u6625\u5170\u7387\u9886\u7684\u4e2d\u592e\u6307\u5bfc\u7ec4 \uff0c\u5e26\u6765\u4e86 \u515a\u4e2d\u592e\u5173\u4e8e\u75ab\u60c5\u9632\u63a7\u7684\u201c\u5b9a\u76d8\u661f \u201d', 'spo_list': [('\u5b59\u6625\u5170\u7387\u9886\u7684\u4e2d\u592e\u6307\u5bfc\u7ec4', '\u5e26\u6765', '\u515a\u4e2d\u592e\u5173\u4e8e\u75ab\u60c5\u9632\u63a7\u7684\u201c\u5b9a\u76d8\u661f')]}, {'text': ' \u8fd9\u6b21\u4f1a\u8bae \u5f62\u6210\u7684\u683c\u5c40\uff0c\u6210\u4e3a\u4e24\u5e74\u540e\u5728\u515a\u7684\u4e03\u5927\u4e0a\u6b63\u5f0f\u5f62\u6210 \u201c\u6bdb\u5218\u5468\u6731\u4efb\u201d\u4e2d\u592e\u9886\u5bfc\u6838\u5fc3\u7684\u96cf\u5f62 ', 'spo_list': [('\u8fd9\u6b21\u4f1a\u8bae', '\u5f62\u6210', '\u201c\u6bdb\u5218\u5468\u6731\u4efb\u201d\u4e2d\u592e\u9886\u5bfc\u6838\u5fc3\u7684\u96cf\u5f62')]}, {'text': '  \u4e2d\u56fd\u75be\u63a7\u4e2d\u5fc3\u7b49\u673a\u6784  \u5728 \u7f8e\u56fd \u300a\u65b0\u82f1\u683c\u5170\u533b\u5b66\u6742\u5fd7\u300b\u4e0a\u53d1\u8868\u300a \u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u7684\u80ba\u708e\u5728\u4e2d\u56fd\u6b66\u6c49\u65e9\u671f\u4f20\u64ad\u52a8\u529b\u5b66 \u300b\uff0c\u901a\u8fc7\u5bf9425\u4f8b\u65b0\u51a0\u80ba\u708e\u786e\u8bca\u60a3\u8005\u6570\u636e\u7814\u7a76\uff0c\u63ed\u793a\u4e86\u75ab\u60c5\u53d1\u751f\u81f31\u670822\u65e5\u65b0\u51a0\u75c5\u6bd2\u7684\u4f20\u64ad\u89c4\u5f8b', 'spo_list': [('\u4e2d\u56fd\u75be\u63a7\u4e2d\u5fc3\u7b49\u673a\u6784', '\u53d1\u8868', '\u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u7684\u80ba\u708e\u5728\u4e2d\u56fd\u6b66\u6c49\u65e9\u671f\u4f20\u64ad\u52a8\u529b\u5b66'), ('\u4e2d\u56fd\u75be\u63a7\u4e2d\u5fc3\u7b49\u673a\u6784', '\u5730\u70b9', '\u7f8e\u56fd')]}, {'text': ' \u5317\u4eac\u65f6\u95f49\u670828\u65e514:40 \uff0c \u5973\u6392\u4e16\u754c\u676f\u7b2c10\u8f6e \u7684\u4e00\u573a\u7126\u70b9\u6218\u6253\u54cd\uff0c\u7531\u4e2d\u56fd\u5973\u6392\u5bf9\u9635\u585e\u5c14\u7ef4\u4e9a\u961f\uff0c\u672c\u573a\u6bd4\u8d5b\u4e2d\u56fd\u5973\u6392\u53ea\u8981\u53d6\u80dc\u5c31\u80fd\u63d0\u524d\u4e00\u8f6e\u593a\u51a0\u3002\u6700\u7ec8\u53cc\u65b9\u7ecf\u8fc73\u5c40\u8f83\u91cf\uff0c\u4e2d\u56fd\u5973\u63923-0\u8f7b\u677e\u53d6\u80dc\uff0c\u63d0\u524d\u4e00\u8f6e\u536b\u5195\u6210\u529f\u3002', 'spo_list': [('\u5317\u4eac\u65f6\u95f49\u670828\u65e514:40', '\u53d6\u80dc', '\u5973\u6392\u4e16\u754c\u676f\u7b2c10\u8f6e')]}, {'text': '\u300a \u4e09\u56fd\u6f14\u4e49 \u300b\u4e2d\u201c \u5173\u7fbd \u65a9\u989c\u826f\u8bdb\u6587\u4e11\u201d\u662f\u5168\u6587\u7ecf\u5178\u7684\u4e00\u4e2a\u6545\u4e8b\u6865\u6bb5\uff0c\u540e\u6765\u8bfb\u8005\u8ba4\u4e3a\uff0c\u6587\u4e11\u548c\u989c\u826f\u90fd\u662f\u8d6b\u8d6b\u6709\u540d\u7684\u6b66\u5c06\uff0c\u4e0d\u53ef\u80fd\u8f7b\u6613\u88ab \u5173\u7fbd \u8bdb\u6740', 'spo_list': [('\u4e09\u56fd\u6f14\u4e49', '\u4e3b\u89d2', '\u5173\u7fbd')]}, {'text': '\u4e09\u3001 \u98df\u7269\u548c\u5455\u5410\u7269\u5438\u5165 \u9664\u98df\u7269\u672c\u8eab\u7684\u523a\u6fc0\u5916\uff0c\u53cd\u6d41\u7684\u80c3\u9178\u4ea6\u662f\u80ba\u635f\u4f24\u7684\u91cd\u8981\u51b3\u5b9a\u56e0\u7d20\u3002\u4e25\u91cd\u8005 \u6c14\u7ba1\u5185\u5438\u5f15\u548c\u673a\u68b0\u901a\u6c14 \u3002', 'spo_list': [('\u98df\u7269\u548c\u5455\u5410\u7269\u5438\u5165', '\u8f85\u52a9\u6cbb\u7597', '\u6c14\u7ba1\u5185\u5438\u5f15\u548c\u673a\u68b0\u901a\u6c14')]}, {'text': '\u201d \u6fb3\u5927\u5229\u4e9a\u5916\u4ea4\u754c\u4e00\u4e9b\u91cd\u8981\u4eba\u7269 \u4e5f\u7eb7\u7eb7\u5bf9\u8be5\u673a\u6784\u63d0\u51fa \u6279\u8bc4 ', 'spo_list': [('\u6fb3\u5927\u5229\u4e9a\u5916\u4ea4\u754c\u4e00\u4e9b\u91cd\u8981\u4eba\u7269', '\u63d0\u51fa', '\u6279\u8bc4')]}, {'text': '\u7530\u6ce2\u3001 \u6881\u5b50\u5b89 \u63a5\u53d7 \u8bf7\u6258 \u540e\uff0c\u4e3a\u4e8c\u4eba\u51fa\u8c0b\u5212\u7b56\uff0c\u5e76\u5728\u6848\u4ef6\u529e\u7406\u8fc7\u7a0b\u4e2d\u5f87\u79c1\u6789\u6cd5\uff0c\u6545\u610f\u8fdd\u80cc\u4e8b\u5b9e\u548c\u6cd5\u5f8b\uff0c\u8fdd\u53cd\u89c4\u5b9a\u4e3a\u5b59\u5c0f\u679c\u7533\u8bc9\u518d\u5ba1\u7acb\u6848\u53ca\u5ba1\u7406\u63d0\u4f9b\u5e2e\u52a9', 'spo_list': [('\u6881\u5b50\u5b89', '\u63a5\u53d7', '\u8bf7\u6258')]}, {'text': '  \u7231\u5fb7\u534e\u591a\u00b7\u6208\u5854\u5c14\u8fea   1985\u5e747\u670822\u65e5 \u51fa\u751f\u4e8e\u5df4\u897f\uff0c\u8eab\u9ad8 193\u5398\u7c73 \uff0c\u60ef\u7528\u811a\u53f3\u811a\uff0c\u4e3b\u8981\u6548\u529b\u4e8e\u83b1\u91cc\u4e9a\u8054\u76df\u4ff1\u4e50\u90e8\uff0c\u573a\u4e0a\u4f4d\u7f6e\u95e8\u5c06', 'spo_list': [('\u7231\u5fb7\u534e\u591a\u00b7\u6208\u5854\u5c14\u8fea', '\u8eab\u9ad8', '193\u5398\u7c73'), ('\u7231\u5fb7\u534e\u591a\u00b7\u6208\u5854\u5c14\u8fea', '\u51fa\u751f\u65e5\u671f', '1985\u5e747\u670822\u65e5')]}, {'text': '\u4e00\u6b21\u62a2\u7b54\u6e38\u620f\u89c4\u5b9a\uff0c\u7b54\u5bf9\u4e00\u9898\u52a015\u5206\uff0c\u7b54\u9519\u4e00\u9898\u8981\u62638\u5206\uff0c\u5c0f\u534e\u62a2\u7b54\u4e8618\u9898\uff1b\u6700\u540e\u5f97\u5206\u662f40\u5206\uff0c\u5979\u7b54\u5bf9\u4e86\u591a\u5c11\u9898\uff0e', 'spo_list': [['\u5c0f\u534e', '\u62a2\u7b54', '\u9898'], ['\u5c0f\u534e', '\u53c2\u52a0', '\u62a2\u7b54\u6e38\u620f'], ['\u62a2\u7b54\u6e38\u620f', '\u5177\u6709', '\u89c4\u5b9a']]}, {'text': '2\u67084\u65e5\uff0c \u6c5f\u897f\u7701\u9e70\u6f6d\u5e02\u4f59\u6c5f\u533a\u5e02\u573a\u76d1\u7ba1\u5c40 \u63a5\u5230 \u4e3e\u62a5 \uff0c\u53cd\u6620\u5728\u6dd8\u5b9d\u5e73\u53f0\u4e0a\u8d2d\u4e70\u9632\u62a4\u53e3\u7f69\uff0c\u6536\u8d27\u540e\u53d1\u73b0\u8be5\u6279\u6b21\u53e3\u7f69\u751f\u4ea7\u65e5\u671f\u4e3a2\u670816\u65e5', 'spo_list': [('\u6c5f\u897f\u7701\u9e70\u6f6d\u5e02\u4f59\u6c5f\u533a\u5e02\u573a\u76d1\u7ba1\u5c40', '\u63a5\u5230', '\u4e3e\u62a5')]}, {'text': '\u80e1\u90e8\u957f\u4ecb\u7ecd\uff0c \u4eba\u957f\u65f6\u95f4\u65c5\u884c \u4f1a\u9020\u6210 \u75b2\u52b3 \uff0c\u539f\u56e0\u4e4b\u4e00\u5c31\u662f\u566a\u97f3', 'spo_list': [('\u4eba\u957f\u65f6\u95f4\u65c5\u884c', '\u9020\u6210', '\u75b2\u52b3')]}, {'text': ' \u56f4\u7532\u6d59\u6c5f\u961f 4-0\u5168\u80dc \u5929\u6d25 \uff0c\u5916\u63f4\u91d1\u5fd7\u9521\u72b6\u6001\u56de\u5f52', 'spo_list': [('\u56f4\u7532\u6d59\u6c5f\u961f', '\u5168\u80dc', '\u5929\u6d25')]}, {'text': ' \u65b0\u52a0\u5761 \u7684\u5b98\u65b9\u6587\u5b57\u4e3a\u82f1\u6587\uff0c\u56e0\u6b64\u516c\u51fd\u3001\u5546\u52a1\u5f80\u6765\u548c\u5176\u4ed6\u7ecf\u6d4e\u4e1a\u52a1\u6027\u8d28\u7684\u4e66\u4fe1\u901a\u5e38\u4ee5 \u82f1\u8bed \u4e3a\u4e3b', 'spo_list': [('\u65b0\u52a0\u5761', '\u5b98\u65b9\u8bed\u8a00', '\u82f1\u8bed')]}, {'text': ' \u6e2f\u6d66\u751f\u7269\u79d1\u6280\uff08\u4e0a\u6d77\uff09\u6709\u9650\u516c\u53f8 \u4e0e\u6211\u56fd\u8457\u540d\u559c\u5267\u6f14\u5458 \u6881\u5929 \u5148\u751f\u4e0e2014\u5e7412\u670815\u65e5\u6b63\u5f0f\u7b7e\u7ea6\uff0c \u6881\u5929 \u5148\u751f\u6210\u4e3a\u6e2f\u6d66\u751f\u7269\u65d7\u4e0b\u4ea7\u54c1\u7684\u5f62\u8c61\u4ee3\u8a00\u4eba', 'spo_list': [('\u6e2f\u6d66\u751f\u7269\u79d1\u6280\uff08\u4e0a\u6d77\uff09\u6709\u9650\u516c\u53f8', '\u4ee3\u8a00\u4eba', '\u6881\u5929')]}, {'text': ' \u4e16\u754c\u519b\u4eba\u8fd0\u52a8\u4f1a  \u6cb3\u5317\u7c4d\u8fd0\u52a8\u5458\u91d1\u6cf3\u5fb7\u643a\u961f\u53cb \u6458\u9996\u91d1', 'spo_list': [('\u6cb3\u5317\u7c4d\u8fd0\u52a8\u5458\u91d1\u6cf3\u5fb7\u643a\u961f\u53cb', '\u6458\u9996\u91d1', '\u4e16\u754c\u519b\u4eba\u8fd0\u52a8\u4f1a')]}, {'text': ' \u4f26\u6566\u5e03\u5170\u7279\u671f\u6cb9 \u5728 \u661f\u671f\u4e00(9\u670816\u65e5) \u5728\u4e9a\u6d32\u65f6\u6bb5\u66fe\u98da\u534719.5%\u621612\u7f8e\u5143\uff0c\u81f3\u6bcf\u687671.45\u7f8e\u5143\uff0c\u6309\u7f8e\u5143\u8ba1\u521b\u51fa\u4e86\u8be5\u5408\u7ea61988\u5e74\u63a8\u51fa\u4ee5\u6765\u7684\u6700\u5927\u5355\u65e5\u6da8\u5e45\uff0c\u521b\u4e09\u5e74\u591a\u6765\u6cb9\u4ef7\u7684\u6700\u9ad8\u8bb0\u5f55\u3002', 'spo_list': [('\u4f26\u6566\u5e03\u5170\u7279\u671f\u6cb9', '\u98da\u5347', '\u661f\u671f\u4e00(9\u670816\u65e5)')]}, {'text': '\u540c\u65f6\uff0c \u4eca\u5e74\u6625\u665a \u4f53\u73b0\u793e\u4f1a\u70ed\u70b9\uff0c\u5173\u6ce8\u594b\u6218\u5728 \u6297\u51fb\u75ab\u60c5\u7b2c\u4e00\u7ebf\u7684\u533b\u52a1\u5de5\u4f5c\u8005 \u3001\u8001\u767e\u59d3\uff0c\u4f20\u9012\u4e86\u4f17\u5fd7\u6210\u57ce\u7684\u575a\u5b9a\u4fe1\u5fc3\u4e0e\u51b3\u5fc3', 'spo_list': [('\u4eca\u5e74\u6625\u665a', '\u5173\u6ce8', '\u6297\u51fb\u75ab\u60c5\u7b2c\u4e00\u7ebf\u7684\u533b\u52a1\u5de5\u4f5c\u8005')]}, {'text': '\u4f60\u76f8\u4fe1\u5929\u6c14\u708e\u70ed\u80fd\u5e2e\u8b66\u65b9\u7834\u6848\u5417\uff1f\u8fd1\u6765\uff0c \u6c5f\u5317\u8b66\u65b9 \u5728\u8c03\u67e5\u4e00\u8d77\u76d7\u7a83\u6848\u65f6\uff0c \u5acc\u7591\u4eba \u5c31\u56e0\u4e3a\u5929\u6c14\u592a\u70ed\uff0c\u9732\u51fa\u4e86\u9a6c\u811a\uff0c\u6700\u540e\u88ab\u6c11\u8b66\u6210\u529f\u6293\u83b7\u3002', 'spo_list': [('\u5acc\u7591\u4eba', '\u6293\u83b7', '\u6c5f\u5317\u8b66\u65b9')]}, {'text': '\u91d1\u878d\u79d1\u6280\u6218\u201c\u75ab\u201d2\u6708\u521d\uff0c \u4e2d\u56fd\u4eba\u6c11\u94f6\u884c\u7b495\u90e8\u95e8 \u4e0b\u53d1 \u300a\u5173\u4e8e\u8fdb\u4e00\u6b65\u5f3a\u5316\u91d1\u878d\u652f\u6301\u9632\u63a7\u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u80ba\u708e\u75ab\u60c5\u7684\u901a\u77e5\u300b \uff0c\u660e\u786e\u63d0\u51fa\u4e86\u91d1\u878d\u652f\u6301\u75ab\u60c5\u9632\u63a7\u4e94\u5927\u65b9\u9762\u5171\u8ba130\u9879\u5177\u4f53\u63aa\u65bd', 'spo_list': [('\u4e2d\u56fd\u4eba\u6c11\u94f6\u884c\u7b495\u90e8\u95e8', '\u4e0b\u53d1', '\u300a\u5173\u4e8e\u8fdb\u4e00\u6b65\u5f3a\u5316\u91d1\u878d\u652f\u6301\u9632\u63a7\u65b0\u578b\u51a0\u72b6\u75c5\u6bd2\u611f\u67d3\u80ba\u708e\u75ab\u60c5\u7684\u901a\u77e5\u300b')]}, {'text': ' \u6e56\u6c9f\u6751 \u4f4d\u4e8e\u5b89\u5fbd\u7701\u6dee\u5317\u5e02\u6fc9\u6eaa\u53bf\u4e34\u6da3\u9547\uff0c\u5168\u6751\u571f\u5730\u9762\u79ef10266\u4ea9\uff0c\u4eba\u53e3 5232\u4eba ', 'spo_list': [('\u6e56\u6c9f\u6751', '\u4eba\u53e3\u6570\u91cf', '5232\u4eba')]}, {'text': '\u5317\u4eac\u65f6\u95f45\u67082\u65e5\u51cc\u6668\uff0c \u5df4\u8428 3-0\u5b8c\u80dc \u5229\u7269\u6d66 \uff0c\u5360\u5f97\u6b27\u51a0\u534a\u51b3\u8d5b\u5148\u673a\uff0c\u6885\u897f\u5b8c\u6210\u6885\u5f00\u4e8c\u5ea6+ \u5df4\u8428 \u751f\u6daf\u7b2c600\u7403\u6210\u5c31\u3002', 'spo_list': [('\u5df4\u8428', '\u5b8c\u80dc', '\u5229\u7269\u6d66')]}, {'text': '\u91d1\u5eb8\u7b2c\u4e8c\u6b21\u56de\u5230\u5185\u5730\u662f\uff11\uff19\uff15\uff13\u5e74\uff0c\u5f53\u65f6  \u4ed6  \u8981\u8fd4\u56de \u676d\u5dde\u548c\u4e0a\u6d77 \u5904\u7406\u4e0e \u524d\u59bb\u675c\u6cbb\u82ac\u7684\u79bb\u5a5a\u4e8b\u5b9c ', 'spo_list': [('\u4ed6', '\u5904\u7406', '\u524d\u59bb\u675c\u6cbb\u82ac\u7684\u79bb\u5a5a\u4e8b\u5b9c'), ('\u4ed6', '\u5730\u70b9', '\u676d\u5dde\u548c\u4e0a\u6d77')]}, {'text': '\u4e9a\u6797\u6240\u79d1\u7814\u5904\u5904\u957f\u5434\u7edf\u8d35\u4ecb\u7ecd\uff0c\u7ecf\u538650\u591a\u5e74\u53d1\u5c55\uff0c \u4e9a\u6797\u6240\u79d1\u7814\u8303\u56f4 \u4ece\u6cb9\u8336\u3001\u6cb9\u6850\u3001\u6bdb\u7af9\u7684\u80b2\u79cd\u548c\u4e30\u4ea7\u683d\u57f9\u6280\u672f\u7b49\uff0c\u5df2\u6269\u5c55\u5230 \u6797\u6728\u80b2\u79cd\u3001\u4fdd\u80b2\u6027\u82d7\u5703\u3001\u56f0\u96be\u7eff\u5730\u4fee\u590d\u3001\u751f\u6001\u6e7f\u5730\u4fdd\u80b2\u7b49\u9886\u57df ', 'spo_list': [('\u4e9a\u6797\u6240\u79d1\u7814\u8303\u56f4', '\u6269\u5c55\u5230', '\u6797\u6728\u80b2\u79cd\u3001\u4fdd\u80b2\u6027\u82d7\u5703\u3001\u56f0\u96be\u7eff\u5730\u4fee\u590d\u3001\u751f\u6001\u6e7f\u5730\u4fdd\u80b2\u7b49\u9886\u57df')]}, {'text': ' \u5965\u65af\u5854\u5f6d\u79d1 \u5728\u597d\u4e0d\u5bb9\u6613\u6253\u51fa\u4e00\u573a2-1\u9006\u8f6c \u65af\u8482\u82ac\u65af \u7684\u8d5b\u5b63\u6700\u4f73\u6218\u7ee9\u540e\uff0c\u7d27\u63a5\u7740\u5c31\u5012\u5728\u4e86\u4f24\u75c5\u9762\u524d\u3002\u5728\u4f0a\u65af\u7279\u672c\u9876\u7ea7\u8d5b\u7684\u5973\u5355\u7b2c\u4e09\u8f6e\uff0c\u8fd9\u4f4d\u62c9\u8131\u7ef4\u4e9a\u5c11\u5973\u5728\u4e0e\u4e9a\u5386\u5c71\u5fb7\u6d1b\u5a03\u7684\u7b2c\u4e8c\u76d8\u8f83\u91cf\u4e2d\u9057\u61be\u9009\u62e9\u9000\u8d5b\uff0c\u6bd4\u5206\u5b9a\u683c\u4e3a6-3\u548c2-1\u3002\u5982\u6b64\u4e00\u6765\uff0c\u5965\u80d6\u5728\u8d5b\u5b63\u81f3\u4eca\u768416\u7ad9\u6bd4\u8d5b\u4e2d\u59cb\u7ec8\u8fc8\u4e0d\u8fc7\u4e09\u8fde\u80dc\u8fd9\u9053\u574e\uff0c\u800c\u5de1\u56de\u8d5b\u6218\u7ee9\u4e3a\u60e8\u6de1\u768411\u80dc16\u8d1f\uff0c\u6392\u540d\u4e5f\u9010\u6e10\u8dcc\u843d\u81f3\u7b2c37\u4f4d\u3002', 'spo_list': [('\u5965\u65af\u5854\u5f6d\u79d1', '\u9006\u8f6c', '\u65af\u8482\u82ac\u65af')]}, {'text': ' 9\u67088\u65e5 \uff0c\u5728\u521a\u521a\u7ed3\u675f\u7684 2019\u7bee\u7403\u4e16\u754c\u676f\u6392\u4f4d\u8d5bm\u7ec4\u6bd4\u8d5b \u4e2d\uff0c\u4e2d\u56fd\u7537\u7bee73-86\u8d1f\u4e8e\u5c3c\u65e5\u5229\u4e9a\u961f\u3002\u800c\u4f0a\u671795-75\u5927\u80dc\u83f2\u5f8b\u5bbe\u3002', 'spo_list': [('9\u67088\u65e5', '\u8d1f\u4e8e', '2019\u7bee\u7403\u4e16\u754c\u676f\u6392\u4f4d\u8d5bm\u7ec4\u6bd4\u8d5b')]}, {'text': '\u4eca\u5929\u4e0b\u5348\uff0c \u5c24\u6587\u56fe\u65af \u5b98\u5ba3\u4e86\u4e00\u7b14\u91cd\u78c5\u4ea4\u6613\uff0c\u7403\u961f\u7b7e\u4e0b\u4e86 \u5e74\u4ec519\u5c81\u7684\u8377\u5170\u56fd\u811a\u5fb7\u91cc\u8d6b\u7279 \u3002', 'spo_list': [('\u5e74\u4ec519\u5c81\u7684\u8377\u5170\u56fd\u811a\u5fb7\u91cc\u8d6b\u7279', '\u7b7e\u4e0b', '\u5c24\u6587\u56fe\u65af')]}, {'text': '\u5728\u626b\u9ed1\u9664\u6076\u4e13\u9879\u6597\u4e89\u4e2d\uff0c \u5546\u90fd\u53bf\u516c\u5b89\u5c40 \u7ecf\u7f1c\u5bc6\u4fa6\u67e5\uff0c\u91cd\u62f3\u51fa\u51fb\uff0c\u4e00\u4e3e\u6253\u6389\u4ee5\u4efb\u5fe0\u4e3a\u9996\u7684\u5bb6\u65cf\u72af\u7f6a\u56e2\u4f19\uff0c\u76ee\u524d \u4e3b\u8981\u72af\u7f6a\u5acc\u7591\u4eba\u4efb\u5fe0 \u3001\u6881\u7231\u82b1\uff08\u4efb\u5fe0\u59bb\u5b50\uff09\u3001\u4efb\u5fd7\u521a\uff08\u4efb\u5fe0\u513f\u5b50\uff09\u7ecf\u5546\u90fd\u53bf\u4eba\u6c11\u68c0\u5bdf\u9662\u6279\u51c6\u4f9d\u6cd5\u6267\u884c\u902e\u6355\u3002', 'spo_list': [('\u5546\u90fd\u53bf\u516c\u5b89\u5c40', '\u902e\u6355', '\u4e3b\u8981\u72af\u7f6a\u5acc\u7591\u4eba\u4efb\u5fe0')]}, {'text': ' rng \u4e0d\u654c fpx \uff0c\u4e24\u4e2a\u81f4\u547d\u5f31\u70b9\u6025\u9700\u89e3\u51b3', 'spo_list': [('rng', '\u4e0d\u654c', 'fpx')]}, {'text': '\u300a     \u767d\u8272\u68a6\u5e7b     \u300b\u662f\u4e00\u90e8\u4e8e1998\u5e741\u67081\u65e5\u51fa\u54c1\u7684\u7535\u89c6\u5267\uff0c\u7531 \u592a\u7eb2 \u5bfc\u6f14\uff0c\u7531 \u7530\u5cb7 \u3001 \u8bb8\u4e9a\u519b \u3001 \u76d6\u4e3d\u4e3d \u548c \u4f55\u6674 \u4e3b\u6f14\uff0c\u4e00\u5171\u670920\u96c6\uff0c\u6bcf\u96c648\u5206\u949f', 'spo_list': [('\u767d\u8272\u68a6\u5e7b', '\u4e3b\u6f14', '\u7530\u5cb7'), ('\u767d\u8272\u68a6\u5e7b', '\u5bfc\u6f14', '\u592a\u7eb2'), ('\u767d\u8272\u68a6\u5e7b', '\u4e3b\u6f14', '\u4f55\u6674'), ('\u767d\u8272\u68a6\u5e7b', '\u4e3b\u6f14', '\u8bb8\u4e9a\u519b'), ('\u767d\u8272\u68a6\u5e7b', '\u4e3b\u6f14', '\u76d6\u4e3d\u4e3d')]}, {'text': '\u4e00\u65e54\u7b7e\uff01\u5c71\u4e1c\u5b98\u5ba3\u7b7e\u4e0b\u585e\u5c14\u767b\uff0c \u8fbd\u7bee \u6b22\u9001 \u536b\u731b \uff0c\u5317\u4eac\u5b98\u5ba3\u53cc\u5916\u63f4\uff01', 'spo_list': [('\u536b\u731b', '\u6b22\u9001', '\u8fbd\u7bee')]}, {'text': '4\u67081\u65e5\uff0c \u6e56\u5317\u7701 \u536b\u5065\u59d4\u5ba3\u5e03\uff0c\u81ea\u4eca\u65e5\u8d77\u6bcf\u65e5\u901a\u62a5\u65e0\u75c7\u72b6\u611f\u67d3\u8005\u60c5\u51b5\uff0c\u622a\u6b62\u52304\u67088\u65e5\uff0c \u6e56\u5317\u7701 \u7d2f\u8ba1\u901a\u62a5 \u65e0\u75c7\u72b6\u611f\u67d3\u8005303\u4f8b ', 'spo_list': [('\u6e56\u5317\u7701', '\u901a\u62a5', '\u65e0\u75c7\u72b6\u611f\u67d3\u8005303\u4f8b')]}, {'text': ' \u6559\u80b2\u90e8 \u8ba4\u771f\u843d\u5b9e \u4e60\u8fd1\u5e73\u603b\u4e66\u8bb0\u91cd\u8981\u6307\u793a\u7cbe\u795e\u548c\u515a\u4e2d\u592e\u51b3\u7b56\u90e8\u7f72 \uff0c\u4e0e\u5916\u4ea4\u90e8\u548c\u9a7b\u5916\u4f7f\u9886\u9986\u4e00\u9053\u624e\u5b9e\u63a8\u8fdb\u5404\u9879\u5de5\u4f5c', 'spo_list': [('\u6559\u80b2\u90e8', '\u843d\u5b9e', '\u4e60\u8fd1\u5e73\u603b\u4e66\u8bb0\u91cd\u8981\u6307\u793a\u7cbe\u795e\u548c\u515a\u4e2d\u592e\u51b3\u7b56\u90e8\u7f72')]}, {'text': '3\u670831\u65e5\u7684\u5916\u4ea4\u90e8\u4f8b\u884c\u8bb0\u8005\u4f1a\u4e0a\uff0c\u53d1\u8a00\u4eba\u534e\u6625\u83b9\u8bc1\u5b9e\uff1a\u6628\u665a\uff0c \u4e1c\u6d77\u6d77\u57df \u53d1\u751f \u4e00\u8d77\u65e5\u672c\u8230\u8247\u4e0e\u4e2d\u56fd\u6e14\u8239\u64e6\u78b0\u4e8b\u6545 \uff0c\u4e00\u540d\u6e14\u6c11\u53d7\u4f24\uff0c\u4e8b\u6545\u539f\u56e0\u6b63\u5728\u8c03\u67e5', 'spo_list': [('\u4e1c\u6d77\u6d77\u57df', '\u53d1\u751f', '\u4e00\u8d77\u65e5\u672c\u8230\u8247\u4e0e\u4e2d\u56fd\u6e14\u8239\u64e6\u78b0\u4e8b\u6545')]}, {'text': '\u5728 hie \u75c5\u56e0\u4e2d\u4ea7\u524d\u548c\u4ea7\u65f6\u7a92\u606f\u5404\u536050%\u548c40%\uff0c\u5176\u4ed6\u539f\u56e0\u7ea6\u536010%\u3002\u3010\u53d1\u75c5\u673a\u5236\u3011\uff08\u4e00\uff09 \u8840\u6d41\u52a8\u529b\u5b66\u53d8\u5316 \u7f3a\u6c27\u65f6\u673a\u4f53\u53d1\u751f\u6f5c\u6c34\u53cd\u5c04\uff0c\u4e3a\u4e86\u4fdd\u8bc1\u91cd\u8981\u751f\u547d\u5668\u5b98\uf