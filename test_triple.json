u654f\u8bda\uff0c\u5f53\u503c\u4e3b\u88c1\u5224\u7ea2\u724c\u7f5a\u4e0b \u803f\u6653\u5cf0 \u3002\u8db3\u534f\u8fd1\u65e5\u5b98\u65b9\u53d1\u6587\u7ed9\u51fa\u4e86\u5bf9 \u803f\u6653\u5cf0 \u7684\u5904\u7f5a\u901a\u77e5\uff0c \u803f\u6653\u5cf0 \u88ab\u7981\u8d5b 3\u573a \u7f5a\u6b3e3\u4e07\u3002', 'spo_list': [('\u803f\u6653\u5cf0', '\u7981\u8d5b', '3\u573a')]}, {'text': '2019\u5e74\u6b66\u6c49\u519b\u8fd0\u4f1a\u5728\u4eca\u65e5\u665a\u95f4\u5f00\u59cb\u6df7\u53cc\u94dc\u724c\u4e89\u593a\u6218\uff0c \u56fd\u4e52\u7ec4\u5408\u5468\u607a/\u5218\u66e6 \u5c06\u5bf9\u9635\u6765\u81ea\u795e\u79d8\u4e4b\u5e08\u671d\u9c9c\u961f\u7684\u7ec4\u5408\u54b8\u6709\u6210/\u8f66\u5b5d\u82af\uff0c\u5728\u4e0a\u4e00\u8f6e\u7684\u6bd4\u8d5b\u4e2d\u4ed6\u4eec\u8f93\u7ed9\u4e86 \u6a0a\u632f\u4e1c/\u6728\u5b50 \u65e0\u7f18\u51b3\u8d5b\uff0c\u800c\u5468\u607a/\u5218\u66e6\u5219\u662f\u6fc0\u6218\u4e03\u5c40\u4e0d\u654c\u961f\u53cb\u5468\u96e8/\u5b59\u94ed\u9633\u6b62\u6b65\u534a\u51b3\u8d5b\uff0c\u56fd\u4e52\u63d0\u524d\u5305\u63fd\u4e86\u6df7\u53cc\u7684\u51a0\u4e9a\u519b\u3002', 'spo_list': [('\u6a0a\u632f\u4e1c/\u6728\u5b50', '\u8f93', '\u56fd\u4e52\u7ec4\u5408\u5468\u607a/\u5218\u66e6')]}, {'text': '\u592e\u89c6\u7f51\u6d88\u606f\uff1a\u6839\u636e\u300a\u4e0a\u6d77\u5e02\u516c\u5171\u573a\u6240\u63a7\u5236\u5438\u70df\u6761\u4f8b\u300b\u89c4\u5b9a\uff0c\u5728\u4eba\u7fa4\u805a\u96c6\u7684\u516c\u5171\u4ea4\u901a\u5de5\u5177\u7b49\u5019\u533a\u57df\u7981\u6b62\u5438\u70df\uff0c\u4f46\u4ecd\u6709\u4e9b\u4eba\u4e0d\u4ee5\u4e3a\u7136\u3002\u8fd1\u65e5\uff0c\u4e0a\u6d77\u7684\u738b\u5148\u751f\u5728\u516c\u4ea4\u8f66\u7ad9\u6392\u961f\u7b49\u8f66\u65f6\uff0c\u9047\u5230\u4e00\u540d\u62bd\u70df\u7684\u7537\u5b50\uff0c\u738b\u5148\u751f\u597d\u5fc3\u529d\u963b\uff0c\u6ca1\u60f3\u5230\u5374\u906d\u5230\u4e86\u5bf9\u65b9\u7684\u6bb4\u6253\u3002\u76ee\u524d\uff0c\u8fd9\u540d \u62bd\u70df\u7537\u5b50 \u56e0\u6d89\u5acc\u5bfb\u8845\u6ecb\u4e8b\u7f6a\u5df2\u88ab \u8b66\u65b9 \u4f9d\u6cd5\u5211\u4e8b\u62d8\u7559\u3002', 'spo_list': [('\u62bd\u70df\u7537\u5b50', '\u62d8\u7559', '\u8b66\u65b9')]}, {'text': '11\u67087\u65e5\uff0c \u4e91\u5357\u7701\u4ea4\u901a\u8fd0\u8f93\u5385 \u516c\u5e03\u4e86 \u6e1d\u6606\u9ad8\u94c1\u9879\u76ee\u73af\u8bc4\u62a5\u544a \uff0c\u540c\u5929\uff0c\u8d35\u9633\u5e02\u8f68\u9053\u4ea4\u901as1\u7ebf\u4e00\u671f\u5de5\u7a0b\u9879\u76ee\u4e5f\u5f00\u5de5\u5efa\u8bbe', 'spo_list': [('\u4e91\u5357\u7701\u4ea4\u901a\u8fd0\u8f93\u5385', '\u516c\u5e03', '\u6e1d\u6606\u9ad8\u94c1\u9879\u76ee\u73af\u8bc4\u62a5\u544a')]}, {'text': ' \u7279\u6717\u666e \u57288\u65e5\u7684\u8bb0\u8005\u4f1a\u4e0a\u4e5f\u5bf9\u6b64\u505a\u51fa\u4e86 \u8bc4\u8bba \uff1a\u201c\u6211\u4e0d\u77e5\u9053\u5965\u5df4\u9a6c\u603b\u7edf\u4e3a\u4f55\u6ca1\u6709\u516c\u5f00\u4ed6\u5bf9\u62dc\u767b\u7684\u652f\u6301', 'spo_list': [('\u7279\u6717\u666e', '\u505a\u51fa', '\u8bc4\u8bba')]}, {'text': ' \u7687\u9a6c \u60e8\u8d25 \u9a6c\u7ade \uff0c\u8c01\u6ce8\u610f\u66ff\u8865\u5e2d\uff1f\u8d1d\u5c14\u5077\u7b11\u6697\u8bbd\uff0c\u9f50\u8fbe\u5185\u53c9\u8170\u65e0\u5948', 'spo_list': [('\u7687\u9a6c', '\u60e8\u8d25', '\u9a6c\u7ade')]}, {'text': ' \u4e91\u516c\u6c11 \u5219\u57282001\u5e747\u6708\u79bb\u5f00 \u5185\u8499\u53e4 \uff0c\u53bb\u4e86\u5c71\u897f\uff0c\u5b98\u81f3\u5c71\u897f\u7701\u59d4\u526f\u4e66\u8bb0\u3001\u5ba3\u4f20\u90e8\u957f\uff0c\u4e4b\u540e\u53c8\u201c\u4ed5\u800c\u4f18\u5219\u5546\u201d\uff0c\u8f6c\u53bb\u4e86\u5927\u540d\u9f0e\u9f0e\u7684\u795e\u534e\u96c6\u56e2', 'spo_list': [('\u4e91\u516c\u6c11', '\u79bb\u5f00', '\u5185\u8499\u53e4')]}, {'text': ' \u51cc\u6f47\u8083 \u53c2\u52a0 \u79cb\u74f7\u70ab \u8865\u529e\u7684\u5a5a\u793c\uff0c\u88ab\u7f51\u53cb\u8c03\u4f83\u66fe\u7ecf\u7684\u4e16\u8d24\u53d8\u5927\u53d4', 'spo_list': [('\u51cc\u6f47\u8083', '\u5a5a\u793c', '\u79cb\u74f7\u70ab')]}, {'text': '\u79d1\u5b66\u4ef7\u503c \u4f8f\u5112\u72e8  \u7075\u957f\u76ee \u91cc\u4e2a\u4f53\u6700\u6700\u5c0f\u7684\uff0c\u4f53\u5f62\u53ef\u7231', 'spo_list': [('\u4f8f\u5112\u72e8', '\u76ee', '\u7075\u957f\u76ee')]}, {'text': ' \u80c3\u764c @###\u5173\u952e\u68c0\u67e5 \u4e0a\u6d88\u5316\u9053\u5185\u955c\u68c0\u67e5 \u5408\u5e76\u6d3b\u68c0\u662f\u9996\u9009\u521d\u59cb\u8bca\u65ad\u68c0\u67e5\u64cd\u4f5c\u3002 \u80c3\u764c @\u4e00\u65e6\u660e\u786e \u80c3\u764c \u8bca\u65ad\uff0c\u60a3\u8005\u8981\u884c\u8fdb\u4e00\u6b65\u5206\u671f\uff0c\u4ee5\u786e\u5b9a\u75be\u75c5\u7684\u8303\u56f4\u548c\u7a0b\u5ea6\u3002', 'spo_list': [('\u80c3\u764c', '\u5185\u7aa5\u955c\u68c0\u67e5', '\u4e0a\u6d88\u5316\u9053\u5185\u955c\u68c0\u67e5')]}, {'text': '4\u67084\u65e5, \u5f20\u67d0\u3001\u5468\u67d0\u7b499\u540d\u65e0\u75c7\u72b6\u611f\u67d3\u8005 \u4e58\u4fc4\u822asu1702\u822a\u73ed,\u81ea\u83ab\u65af\u79d1\u76f4\u98de\u7b26\u62c9\u8fea\u6c83\u65af\u6258\u514b\uff0c\u4e8e\u6b21\u65e5\u4e58\u5750\u5927\u5df4\u5230\u8fbe \u7ee5\u82ac\u6cb3\u516c\u8def\u53e3\u5cb8 ', 'spo_list': [('\u5f20\u67d0\u3001\u5468\u67d0\u7b499\u540d\u65e0\u75c7\u72b6\u611f\u67d3\u8005', '\u5230\u8fbe', '\u7ee5\u82ac\u6cb3\u516c\u8def\u53e3\u5cb8')]}, {'text': '\u66fe\u6625\u857e\u9a70\u63f4\u65e0\u679c\uff0c\u5b89\u5bb6\u6770\u6307\u6325\u4e4f\u5584\u53ef\u9648\uff0c \u4e2d\u56fd\u5973\u6392 \u5b8c\u8d25 \u5df4\u897f \u518d\u4ea4\u5b66\u8d39', 'spo_list': [('\u4e2d\u56fd\u5973\u6392', '\u5b8c\u8d25', '\u5df4\u897f')]}, {'text': '\u7f8e\u56fd\u75be\u63a7\u4e2d\u5fc3\u4e5f\u6539\u53e3\u5f53\u5730\u65f6\u95f44\u67083\u65e5\uff0c \u7f8e\u56fd\u75be\u75c5\u63a7\u5236\u548c\u9884\u9632\u4e2d\u5fc3 (cdc)\u4fee\u6539\u4e86\u5176\u5173\u4e8e\u7f8e\u56fd\u6c11\u4f17\u4f69\u6234\u53e3\u7f69\u7684\u6307\u5357\uff0c\u5efa\u8bae\u6c11\u4f17 \u5728\u65e0\u6cd5\u4fdd\u6301\u9002\u5f53\u793e\u4ea4\u8ddd\u79bb\u7684\u516c\u5171\u573a\u6240\uff0c\u6234\u4e0a\u975e\u533b\u7528\u53e3\u7f69\uff0c\u4ee5\u907f\u514d\u4f20\u64ad\u65b0\u51a0\u75c5\u6bd2 ', 'spo_list': [('\u7f8e\u56fd\u75be\u75c5\u63a7\u5236\u548c\u9884\u9632\u4e2d\u5fc3', '\u5efa\u8bae', '\u5728\u65e0\u6cd5\u4fdd\u6301\u9002\u5f53\u793e\u4ea4\u8ddd\u79bb\u7684\u516c\u5171\u573a\u6240\uff0c\u6234\u4e0a\u975e\u533b\u7528\u53e3\u7f69\uff0c\u4ee5\u907f\u514d\u4f20\u64ad\u65b0\u51a0\u75c5\u6bd2')]}, {'text': ' \u5317\u4eac\u65f6\u95f48\u670823\u65e5 \uff0c\u7537\u7bee\u4e16\u754c\u676f\u70ed\u8eab\u8d5b\u7ee7\u7eed\u8fdb\u884c\uff0c\u4e2d\u56fd\u7537\u7bee\u8fce\u6218 \u5df4\u897f\u7537\u7bee \u3002\u7ecf\u8fc74\u8282\u7684\u6bd4\u8d5b\uff0c\u4e2d\u56fd\u7537\u7bee\u4ee584:90\uff0c6\u5206\u60dc\u8d25 \u5df4\u897f\u7537\u7bee \u3002', 'spo_list': [('\u5317\u4eac\u65f6\u95f48\u670823\u65e5', '\u60dc\u8d25', '\u5df4\u897f\u7537\u7bee')]}, {'text': '\u6839\u636ewho\u7684 \u5370\u5ea6 \u62a5\u544a\uff0c\u76f4\u52303\u6708\u521d\uff0c \u5370\u5ea6 \u624d\u53d1\u5e03\u5bf9 \u6240\u6709\u56fd\u9645\u5165\u5883\u4e58\u5ba2 \u8fdb\u884c\u5168\u9762\u533b\u7597\u68c0\u67e5', 'spo_list': [('\u5370\u5ea6', '\u68c0\u67e5', '\u6240\u6709\u56fd\u9645\u5165\u5883\u4e58\u5ba2')]}, {'text': ' 10\u670818\u65e5 \uff0c\u7531\u4e2d\u56fd\u56fd\u9645\u8d38\u6613\u4fc3\u8fdb\u59d4\u5458\u4f1a\u3001\u6e56\u5357\u7701\u4eba\u6c11\u653f\u5e9c\u5171\u540c\u4e3b\u529e\u7684 2019\u4e2d\u56fd\u56fd\u9645\u8f68\u9053\u4ea4\u901a\u548c\u88c5\u5907\u5236\u9020\u4e1a\u535a\u89c8\u4f1a \u5728\u957f\u6c99\u56fd\u9645\u4f1a\u5c55\u4e2d\u5fc3\u5f00\u5e55\u3002', 'spo_list': [('10\u670818\u65e5', '\u5f00\u5e55', '2019\u4e2d\u56fd\u56fd\u9645\u8f68\u9053\u4ea4\u901a\u548c\u88c5\u5907\u5236\u9020\u4e1a\u535a\u89c8\u4f1a')]}, {'text': ' \u5df4\u897f 3-1\u79d8\u9c81\u8c6a\u53d6 \u7f8e\u6d32\u676f \u7b2c9\u51a0\uff0c\u7403\u738b\u6885\u897f\u6216\u88ab\u5357\u7f8e\u8db3\u534f\u7981\u8d5b\u4e24\u5e74\uff01', 'spo_list': [('\u5df4\u897f', '\u7b2c9\u51a0', '\u7f8e\u6d32\u676f')]}, {'text': '\u8fd9\u4e09\u5c40\u6bd4\u8d5b\u90fd\u548cig\u6709\u5173\u7cfb\uff0c ig\u6218\u961f \u5728\u672c\u6b21msi\u4e0a\u53d1\u6325\u795e\u52c7\uff0c \u5c0f\u7ec4\u8d5b \u62ff\u52309\u8fde\u80dc\uff0c\u8fd9\u4e2a\u5df2\u7ecf\u662flpl\u53f2\u4e0a\u6700\u597d\u7684msi \u5c0f\u7ec4\u8d5b \u6218\u7ee9\u4e86\u3002', 'spo_list': [('ig\u6218\u961f', '\u8fde\u80dc', '\u5c0f\u7ec4\u8d5b')]}, {'text': ' \u56fd\u9645\u5929\u98df(03666) \u53d1\u5e03\u516c\u544a\uff0c\u4e8e2019\u5e746\u670813\u65e5\uff0c\u8463\u4e8b\u6536\u5230 \u6731\u6653\u971e \u901a\u77e5\uff0c\u7531\u4e8e\u4e0e\u516c\u53f8\u8463\u4e8b\u4f1a\u4e3b\u5e2d\u738b\u6167\u654f\u5728\u5408\u4f5c\u53ca\u7ba1\u7406\u7406\u5ff5\u65b9\u9762\u5b58\u5728\u4e25\u91cd\u610f\u89c1\u5206\u6b67\uff0c\u5bfc\u81f4\u5176\u96be\u4ee5\u6709\u6548\u72ec\u7acb\u5c65\u884c\u6267\u884c\u8463\u4e8b\u804c\u8d23\uff0c\u6545 \u6731\u6653\u971e \u5df2\u8f9e\u4efb\u6267\u884c\u8463\u4e8b\uff0c\u5373\u65f6\u751f\u6548\u3002', 'spo_list': [('\u6731\u6653\u971e', '\u8f9e\u4efb', '\u56fd\u9645\u5929\u98df(03666)')]}, {'text': '12\u65e5\u665a\uff0c \u5468\u6069\u6765 \u5199\u4fe1\u7ed9 \u8584\u4e00\u6ce2 \u8bf4:\u6628\u591c\u5411\u4e3b\u5e2d\u6c47\u62a5\u5f00\u4f1a\u60c5\u5f62\uff0c\u4ed6\u6307\u793a\u9886\u5bfc\u5c0f\u7ec4\u4f1a\u8bae\u5e94\u8be5\u6269\u5927\u4e3e\u884c\uff0c\u4f7f\u5404\u65b9\u9762\u6709\u5173\u540c\u5fd7\u90fd\u80fd\u542c\u5230\u4f60\u7684\u53d1\u8a00\uff0c\u540c\u65f6\u8981\u5c55\u5f00\u684c\u9762\u4e0a\u7684\u6597\u4e89\uff0c\u89e3\u51b3\u95ee\u9898\uff0c\u4e0d\u8981\u91c7\u53d6\u5eb8\u4fd7\u6001\u5ea6\uff0c\u5f53\u9762\u4e0d\u8bf4\u80cc\u540e\u8bf4\uff0c\u4e0d\u76f4\u8bf4\u800c\u7ed5\u5f2f\u5b50\u8bf4\uff0c\u4e0d\u6307\u540d\u8bf4\u800c\u6697\u793a\u5f0f\u8bf4\uff0c\u90fd\u662f\u4e0d\u5bf9\u7684', 'spo_list': [('\u5468\u6069\u6765', '\u5199\u4fe1', '\u8584\u4e00\u6ce2')]}, {'text': ' \u8fd1\u65e5 \uff0c \u671d\u9633\u7fa4\u4f17 \u4e3e\u62a5\u5728\u5317\u4eac\u671d\u9633\u67d0\u9ad8\u6863\u5c0f\u533a\u5185\u6709\u4e00\u6237\u4eba\u5bb6\u6d89\u6bd2\u3002', 'spo_list': [('\u8fd1\u65e5', '\u4e3e\u62a5', '\u671d\u9633\u7fa4\u4f17')]}, {'text': '\u622a\u81f3\u76ee\u524d, \u6d77\u6dc0\u6cd5\u9662 \u5411\u6d77\u6dc0\u516c\u5b89\u5206\u5c40\u62a5\u9001 \u534f\u67e5\u88ab\u6267\u884c\u4eba1100\u591a\u4eba ,\u63a5\u5230\u60c5\u62a5\u53cd\u9988600\u591a\u6761,\u6210\u529f\u5728\u5bbe\u9986\u3001\u7f51\u5427\u548c\u6d17\u6d74\u4e2d\u5fc3\u7b49\u573a\u6240\u627e\u523080\u591a\u540d\u88ab\u6267\u884c\u4eba\uff1b\u5728\u94c1\u8def\u516c\u5b89\u914d\u5408\u4e0b,\u5728\u5317\u4eac\u5404\u706b\u8f66\u7ad9\u53ca\u82cf\u5dde\u3001\u897f\u5b89\u7b49\u5916\u5730\u706b\u8f66\u7ad9\u6210\u529f\u622a\u83b7100\u591a\u4eba', 'spo_list': [('\u6d77\u6dc0\u6cd5\u9662', '\u62a5\u9001', '\u534f\u67e5\u88ab\u6267\u884c\u4eba1100\u591a\u4eba')]}, {'text': '\u957f\u57ce\u6c7d\u8f66\u8fd8\u5728 \u6e56\u5317 \u63a8\u51fa \u4e13\u5c5e\u670d\u52a1\u4e3e\u63aa \uff0c\u4e0e\u8f66\u4e3b\u5171\u7b51\u6297\u75ab\u201c\u957f\u57ce\u201d', 'spo_list': [('\u6e56\u5317', '\u63a8\u51fa', '\u4e13\u5c5e\u670d\u52a1\u4e3e\u63aa')]}, {'text': '\u4e2d\u5171\u4e2d\u592e\u653f\u6cbb\u5c40\u59d4\u5458\u3001\u4e2d\u592e\u4e66\u8bb0\u5904\u4e66\u8bb0\uff0c\u5168\u56fd\u4eba\u5927\u5e38\u59d4\u4f1a\u526f\u59d4\u5458\u957f\uff0c\u56fd\u52a1\u59d4\u5458\uff0c\u6700\u9ad8\u4eba\u6c11\u6cd5\u9662\u9662\u957f\uff0c\u6700\u9ad8\u4eba\u6c11\u68c0\u5bdf\u9662\u68c0\u5bdf\u957f\uff0c \u5168\u56fd\u653f\u534f\u526f\u4e3b\u5e2d \uff0c\u4ee5\u53ca\u4e2d\u592e\u519b\u59d4\u59d4\u5458\u5c31\u8fd1\u5728\u5de5\u4f5c\u5730\u70b9\u53c2\u52a0 \u54c0\u60bc\u6d3b\u52a8 ', 'spo_list': [('\u5168\u56fd\u653f\u534f\u526f\u4e3b\u5e2d', '\u53c2\u52a0', '\u54c0\u60bc\u6d3b\u52a8')]}, {'text': '\u56e0\u4e3a\u9a9a\u6270\u7535\u8bdd\u95ee\u9898\u7a81\u51fa\uff0c 5\u670817\u65e5 \uff0c \u5de5\u4fe1\u90e8\u4fe1\u606f\u901a\u4fe1\u7ba1\u7406\u5c40 \u9488\u5bf9\u8fd1\u671f\u201c95\u201d\u53f7\u7801\u548c\u79fb\u52a8\u8f6c\u552e\u4e1a\u52a1\u201c170\u201d\u201c171\u201d\u7b49\u53f7\u6bb5\u62e8\u6253\u9a9a\u6270\u7535\u8bdd\u4e25\u91cd\u6270\u6c11\u3001\u7fa4\u4f17\u4e3e\u62a5\u6295\u8bc9\u5c45\u9ad8\u4e0d\u4e0b\u7b49\u7a81\u51fa\u95ee\u9898\uff0c\u5206\u522b\u96c6\u4e2d\u7ea6\u8c0820\u5bb6\u547c\u53eb\u4e2d\u5fc3\u4f01\u4e1a\u548c10\u5bb6\u79fb\u52a8\u8f6c\u552e\u4f01\u4e1a(\u4ee5\u4e0b\u7b80\u79f0\u865a\u5546)\u3002', 'spo_list': [('5\u670817\u65e5', '\u7ea6\u8c08', '\u5de5\u4fe1\u90e8\u4fe1\u606f\u901a\u4fe1\u7ba1\u7406\u5c40')]}, {'text': '\u534a\u573a-\u6768\u4e16\u5143\u4f24\u9000 \u4e0a\u6e2f \u66820-1 \u6d66\u548c \u603b\u6bd4\u52062\u6bd43\u843d\u540e', 'spo_list': [('\u4e0a\u6e2f', '0-1', '\u6d66\u548c')]}, {'text': '\u4e2a\u4eba\u4fe1\u606f\u59d3\u540d\uff1a \u4f0a\u4e07-\u4f69\u6069 erwanperon( \u6cd5\u56fd )\u56fd\u7c4d\uff1a \u6cd5\u56fd \u6027\u522b\uff1a\u7537\u751f\u65e5\uff1a1980', 'spo_list': [('\u4f0a\u4e07-\u4f69\u6069', '\u56fd\u7c4d', '\u6cd5\u56fd')]}, {'text': '\u91d1\u5eb8\u7b2c\u4e8c\u6b21\u56de\u5230\u5185\u5730\u662f1953\u5e74\uff0c\u5f53\u65f6  \u4ed6  \u8981\u8fd4\u56de \u676d\u5dde\u548c\u4e0a\u6d77 \u5904\u7406\u4e0e \u524d\u59bb\u675c\u6cbb\u82ac\u7684\u79bb\u5a5a\u4e8b\u5b9c ', 'spo_list': [('\u4ed6', '\u5904\u7406', '\u524d\u59bb\u675c\u6cbb\u82ac\u7684\u79bb\u5a5a\u4e8b\u5b9c'), ('\u4ed6', '\u5730\u70b9', '\u676d\u5dde\u548c\u4e0a\u6d77')]}, {'text': ' \u8fd1\u65e5 \uff0c \u817e\u8bafwegame\u5e73\u53f0 \u4e0a\u7ebf\u4e86\u4e00\u4e2a\u624b\u673a\u4e32\u6d41\u529f\u80fd\uff0c\u8be5\u529f\u80fd\u53ef\u4ee5\u5c06\u7535\u8111\u548c\u624b\u673a\u4e32\u8fde\uff0c\u5728\u624b\u673a\u4e0a\u5c31\u53ef\u4ee5\u73a9\u7aef\u6e38\uff0c\u76ee\u524d\u8be5\u529f\u80fd\u6b63\u5728\u9884\u7ea6\u4e2d\uff0c\u611f\u5174\u8da3\u7684\u73a9\u5bb6\u53ef\u4ee5\u524d\u53bb\u9884\u7ea6', 'spo_list': [('\u8fd1\u65e5', '\u4e0a\u7ebf', '\u817e\u8bafwegame\u5e73\u53f0')]}, {'text': ' \u6628\u65e5 \uff0c \u9ad8\u5706\u5706 \u4e8e\u5317\u4eac\u4ea7\u5973\u664b\u5347\u5f53\u5988\u5988\uff0c\u8fc5\u901f\u767b\u4e0a\u70ed\u641c\u5168\u7f51\u6cb8\u817e\uff0c\u4f5c\u4e3a\u65b0\u664b\u5976\u7238\u7684\u8d75\u53c8\u5ef7\u4e5f\u662f\u96be\u4ee5\u5fcd\u4f4f\u5185\u5fc3\u7684\u6fc0\u52a8\uff0c\u53d1\u535a\u5206\u4eab\u559c\u60a6:\u7f8e\u597d\u7684\u65e5\u5b50\uff0c\u8fce\u6765\u7f8e\u597d\u7684\u751f\u547d\u3002', 'spo_list': [('\u6628\u65e5', '\u4ea7\u5973', '\u9ad8\u5706\u5706')]}, {'text': '\u65b0\u4eac\u62a5\u8baf\uff08\u8bb0\u8005\u5218\u9633\u5b9e\u4e60\u751f\u6c6a\u6797\uff09\u5e7f\u897f\u4e00\u8f86\u4e8c\u624b\u5bbe\u5229\u8f7f\u8f66\u5728\u7f51\u4e0a\u62cd\u5356\u5f15\u53d1\u70ed\u8bae\u3002\u8fd9\u6b3e\u8d77\u62cd\u4ef786\u4e07\u5143\u7684\u4e8c\u624b\u5bbe\u5229\u5728\u7ecf\u5386\u8fd1160\u6b21\u51fa\u4ef7\u540e\uff0c\u6700\u7ec8\u4ee51436\u4e07\u7684\u5929\u4ef7\u843d\u69cc\uff0c\u4f46\u968f\u540e\u7ade\u62cd\u8005 \u674e\u5973\u58eb \u4ee5\u201c\u64cd\u4f5c\u5931\u8bef\u201d\u6bc1\u62cd\u30029\u670811\u65e5\uff0c\u65b0\u4eac\u62a5\u8bb0\u8005\u7b2c\u4e00\u65f6\u95f4\u81f4\u7535 \u5e7f\u897f\u516c\u52a1\u62cd\u5356\u884c \u6838\u5b9e\uff0c\u5bf9\u65b9\u79f0\u7ecf\u529e\u4eba\u4e0d\u5728\uff0c\u4e0d\u4e86\u89e3\u5177\u4f53\u60c5\u51b5\u3002\u636e\u4e86\u89e3\uff0c \u674e\u5973\u58eb \u4ee5\u201c\u62cd\u5356\u8fc7\u7a0b\u51fa\u73b0\u91cd\u5927\u5931\u8bef\u201d\u4e3a\u7531\u5c06 \u5e7f\u897f\u516c\u52a1\u62cd\u5356\u884c \u8d77\u8bc9\u81f3\u6cd5\u9662\u3002\u6709\u5f8b\u5e08\u5bf9\u65b0\u4eac\u62a5\u8bb0\u8005\u8868\u793a\uff0c \u674e\u5973\u58eb \u662f\u5426\u80fd\u5f97\u5230\u6cd5\u9662\u652f\u6301\uff0c\u8fd8\u9700\u8981\u7ed3\u5408\u62cd\u5356\u54c1\u724c\u7684\u62a5\u4ef7\u89c4\u5f8b\u53ca\u62cd\u5356\u5e38\u7406\u3002', 'spo_list': [('\u674e\u5973\u58eb', '\u8d77\u8bc9', '\u5e7f\u897f\u516c\u52a1\u62cd\u5356\u884c')]}, {'text': '1 \u90ed\u9521\u521a \uff0c\u7956\u7c4d \u5c71\u897f\u592a\u539f\u90ed\u5bb6\u57ce ', 'spo_list': [('\u90ed\u9521\u521a', '\u7956\u7c4d', '\u5c71\u897f\u592a\u539f\u90ed\u5bb6\u57ce')]}, {'text': '  \u8bb0\u8005  \u5728 \u6c5f\u6751 \u627e\u5230\u4e86\u591a\u540d\u66fe\u5728 \u6c5f\u6751 \u6c9f\u5783\u573e\u586b\u57cb\u573a\u62fe\u8352\u4e3a\u751f\u7684 \u8001\u4eba \uff0c\u4ed6\u4eec\u8868\u793a\uff0c\u6700\u591a\u65f6\uff0c\u5783\u573e\u573a\u91cc\u540c\u65f6\u751f\u6d3b\u7740\u6570\u767e\u540d\u62fe\u8352\u8005', 'spo_list': [('\u8bb0\u8005', '\u627e\u5230', '\u8001\u4eba'), ('\u8bb0\u8005', '\u5730\u70b9', '\u6c5f\u6751')]}, {'text': ' \u4e2d\u5171\u4e2d\u592e\u603b\u4e66\u8bb0\u3001\u56fd\u5bb6\u4e3b\u5e2d\u3001\u4e2d\u592e\u519b\u59d4\u4e3b\u5e2d\u4e60\u8fd1\u5e73 \u51fa\u5e2d\u4f1a\u8bae\u5e76\u53d1\u8868 \u91cd\u8981\u8bb2\u8bdd ', 'spo_list': [('\u4e2d\u5171\u4e2d\u592e\u603b\u4e66\u8bb0\u3001\u56fd\u5bb6\u4e3b\u5e2d\u3001\u4e2d\u592e\u519b\u59d4\u4e3b\u5e2d\u4e60\u8fd1\u5e73', '\u53d1\u8868', '\u91cd\u8981\u8bb2\u8bdd')]}, {'text': '\u5f53\u5730\u65f6\u95f44\u67088\u65e5\u4e0a\u5348\uff0c\u975e\u76df\u59d4\u5458\u4f1a\u4e3b\u5e2d\u6cd5\u57fa\u5728\u793e\u4ea4\u5a92\u4f53\u4e0a\u8868\u6001\uff0c\u5bf9\u4e8e \u7f8e\u56fd\u653f\u5e9c \u53cd\u5bf9\u4e16\u754c\u536b\u751f\u7ec4\u7ec7 \u5728\u5168\u7403\u6297\u51fb\u75ab\u60c5\u7684\u9886\u5bfc\u8868\u793a\u9707\u60ca\uff0c\u975e\u76df\u5c06\u5168\u529b\u652f\u6301\u4e16\u754c\u536b\u751f\u7ec4\u7ec7\u548c\u8c2d\u5fb7\u585e ', 'spo_list': [('\u7f8e\u56fd\u653f\u5e9c', '\u53cd\u5bf9', '\u5728\u5168\u7403\u6297\u51fb\u75ab\u60c5\u7684\u9886\u5bfc\u8868\u793a\u9707\u60ca\uff0c\u975e\u76df\u5c06\u5168\u529b\u652f\u6301\u4e16\u754c\u536b\u751f\u7ec4\u7ec7\u548c\u8c2d\u5fb7\u585e')]}, {'text': '\u540c\u65f6\uff0c \u5fd7\u613f\u8005\u4eec \u53c2\u52a0 \u4e34\u5e8a\u5b9e\u9a8c \uff0c\u4e5f\u90fd\u83b7\u5f97\u4e86\u5bb6\u4eba\u7684\u5e94\u5141\u4e0e\u7406\u89e3', 'spo_list': [('\u5fd7\u613f\u8005\u4eec', '\u53c2\u52a0', '\u4e34\u5e8a\u5b9e\u9a8c')]}, {'text': ' \u5229\u7269\u6d66 4-0 \u5df4\u8428 \u590d\u76d8\u8bc4\u8bba\u533a\u53c8\u88ab\u653b\u5360\u4e86', 'spo_list': [('\u5229\u7269\u6d66', '4-0', '\u5df4\u8428')]}, {'text': '\u6628\u5929\uff0c\u5907\u53d7\u5173\u6ce8\u7684 \u7535\u5f71\u300a\u95fb\u70df\u300b \u6b63\u5f0f\u5b9a\u6863 8\u670830\u65e5 \uff0c\u8be5\u7247\u6539\u7f16\u81ea\u4e2d\u56fd\u4f5c\u5bb6\u8f9b\u9149\u540c\u540d\u77ed\u7bc7\u5c0f\u8bf4\uff0c\u7531\u5965\u65af\u5361\u6700\u4f73\u5916\u8bed\u7247\u300a\u5165\u6b93\u5e08\u300b\u5bfc\u6f14\u6cf7\u7530\u6d0b\u4e8c\u90ce\u643a\u539f\u73ed\u5e55\u540e\u56e2\u961f\u5320\u5fc3\u6253\u9020\uff0c\u97e9\u5e9a\u3001\u5f20\u56fd\u7acb\u3001\u8bb8\u6674\u3001\u859b\u660a\u5a67\u4e3b\u6f14\u3002', 'spo_list': [('8\u670830\u65e5', '\u5b9a\u6863', '\u7535\u5f71\u300a\u95fb\u70df\u300b')]}, {'text': '\u4eca\u5e747\u670831\u65e5, \u5b89\u5fbd\u7701\u7eaa\u59d4\u529e\u516c\u5385 \u5370\u53d1 \u300a\u4e3a\u53d7\u5230\u8bec\u544a\u9677\u5bb3\u9519\u544a\u8bef\u544a\u5e72\u90e8\u6f84\u6e05\u6b63\u540d\u82e5\u5e72\u89c4\u5b9a(\u8bd5\u884c)\u300b ,\u7ecf\u6838\u5b9e\u786e\u5c5e\u8bec\u544a\u9677\u5bb3\u3001\u9519\u544a\u8bef\u544a\u884c\u4e3a,\u4e14\u7ed9\u88ab\u53cd\u6620\u4eba\u9020\u6210\u4e0d\u826f\u5f71\u54cd\u7684,\u5e94\u5f53\u4e88\u4ee5\u6f84\u6e05\u6b63\u540d', 'spo_list': [('\u5b89\u5fbd\u7701\u7eaa\u59d4\u529e\u516c\u5385', '\u5370\u53d1', '\u300a\u4e3a\u53d7\u5230\u8bec\u544a\u9677\u5bb3\u9519\u544a\u8bef\u544a\u5e72\u90e8\u6f84\u6e05\u6b63\u540d\u82e5\u5e72\u89c4\u5b9a(\u8bd5\u884c)\u300b')]}, {'text': ' \u4e00\u8f88\u5b50\u4e00\u573a\u68a6 1\u6b4c\u66f2\u9274\u8d4f\u8fd9\u9996\u6b4c\u662f\u300a\u5c0f\u5b9d\u4e0e\u5eb7\u7199\u300b\u7684\u7247\u5934\u66f2\uff0c\u7531 \u674e\u5b89\u4fee \u4f5c\u8bcd\uff0c\u628a\u97e6\u5c0f\u5b9d\u7684\u6027\u683c\u63cf\u8ff0\u7684\u5f88\u900f\u5f7b', 'spo_list': [('\u4e00\u8f88\u5b50\u4e00\u573a\u68a6', '\u4f5c\u8bcd', '\u674e\u5b89\u4fee')]}, {'text': '\u4eca\u65e5\u79d1\u521b\u677f\u5bf9\u6807\u6982\u5ff5\u80a1\u4e09\u8d85\u65b0\u6750\u6536\u83b74\u8fde\u677f\u8d70\u52bf\uff0c\u8be5\u80a1\u7684\u706b\u7206\u5e26\u52a8\u4e86\u80a1\u7968\u7b80\u79f0\u5305\u542b\u201c\u65b0\u6750\u201d\u7684\u80a1\u7968\u96c6\u4f53\u8d70\u5f3a\uff0c\u622a\u81f3\u53d1\u7a3f\uff0c \u5929\u5b89\u65b0\u6750 \u3001 \u4e09\u5ce1\u65b0\u6750 \u3001\u4e3d\u5c9b\u65b0\u6750\u3001\u4f17\u6e90\u65b0\u6750\u6da8\u505c\uff0c\u6d77\u987a\u65b0\u6750\u3001\u9f0e\u76db\u65b0\u6750\u7b49\u6da8\u8d855%\u3002\u4e0e\u6b64\u540c\u65f6\uff0c\u4e3b\u8425\u4e3a\u5203\u5177\u6a21\u5177\u7684\u6052\u950b\u5de5\u5177\u3001\u7ecf\u8425\u8d85\u786c\u6750\u6599\u7684\u9ec4\u6cb3\u65cb\u98ce\u540c\u6837\u6da8\u505c\u3002', 'spo_list': [('\u5929\u5b89\u65b0\u6750', '\u6da8\u505c', '\u4e09\u5ce1\u65b0\u6750')]}, {'text': '\u8fd1\u65e5\uff0c \u5409\u6797\u7701\u4eba\u6c11\u9632\u7a7a\u529e\u516c\u5ba4\u539f\u5de1\u89c6\u5458\u4ef2\u7814(\u6b63\u5385\u7ea7) \u6d89\u5acc\u53d7\u8d3f\u7f6a\u4e00\u6848\uff0c\u7ecf\u5409\u6797\u7701\u4eba\u6c11\u68c0\u5bdf\u9662\u6307\u5b9a\u7ba1\u8f96\uff0c\u7531 \u5409\u6797\u5e02\u4eba\u6c11\u68c0\u5bdf\u9662 \u5411\u5409\u6797\u5e02\u4e2d\u7ea7\u4eba\u6c11\u6cd5\u9662\u63d0\u8d77\u516c\u8bc9\u3002', 'spo_list': [('\u5409\u6797\u5e02\u4eba\u6c11\u68c0\u5bdf\u9662', '\u516c\u8bc9', '\u5409\u6797\u7701\u4eba\u6c11\u9632\u7a7a\u529e\u516c\u5ba4\u539f\u5de1\u89c6\u5458\u4ef2\u7814(\u6b63\u5385\u7ea7)')]}, {'text': '2\u67083\u65e5\uff0c \u6b66\u6c49\u6d88\u8d39\u8005\u53f6\u5973\u58eb \u5728\u6d77\u8c5a\u5bb6app\u8d2d\u4e70\u4e86 3\u76d2x-condi\u53e3\u7f69 \uff0c\u603b\u5171\u82b1\u4e8676\u5143\uff0c\u8bf4\u662f24\u5c0f\u65f6\u53d1\u8d27\uff0c\u4f46\u6b64\u540e\u5979\u4e00\u76f4\u6ca1\u770b\u5230\u7269\u6d41\u4fe1\u606f', 'spo_list': [('\u6b66\u6c49\u6d88\u8d39\u8005\u53f6\u5973\u58eb', '\u8d2d\u4e70', '3\u76d2x-condi\u53e3\u7f69')]}, {'text': '  \u5ddd\u5d0e\u75c5  @\u6709\u5173\u53d1\u75c5\u673a\u5236\u6709\u4e0b\u5217\u5b66\u8bf4\uff1a\uff08\u4e00\uff09 \u514d\u75ab\u8fc7\u5ea6\u6d3b\u5316 \u7814\u7a76\u8ba4\u4e3a\u514d\u75ab\u53cd\u5e94\u4ea2\u8fdb\u662f\u81f4\u75c5\u7684\u91cd\u8981\u73af\u8282\uff0c\u7a81\u51fa\u8868\u73b0\u5728\u6025\u6027\u671fcd30+t\u7ec6\u80de\u589e\u591a\uff0c\u90e8\u5206\u7ec6\u80de\u56e0\u5b50\u5206\u6ccc\u5f02\u5e38\uff0cb\u7ec6\u80de\u591a\u514b\u9686\u6d3b\u5316\uff0c\u5faa\u73af\u4e2d\u6709\u6297\u5185\u76ae\u7ec6\u80de\u6bd2\u6027\u6297\u4f53\u53ca\u6297\u4e2d\u6027\u7c92\u80de\u6d46\u6297\u4f53\u7b49\u4f53\u6db2\u514d\u75ab\u53cd\u5e94\u4ea2\u8fdb\u73b0\u8c61\uff1b\u5f02\u5e38\u589e\u9ad8\u7684il-6\u80fd\u6291\u5236\u6dcb\u5df4\u7ec6\u80dep53\u57fa\u56e0\u8868\u8fbe\uff0c\u6dcb\u5df4\u7ec6\u80de\u51cb\u4ea1\u65f6\u95f4\u660e\u663e\u5ef6\u8fdf\uff0c\u6700\u7ec8\u5bfc\u81f4\u514d\u75ab\u7ec6\u80de\u8fc7\u5ea6\u6d3b\u5316\u3002  \u5ddd\u5d0e\u75c5  @\uff08\u4e8c\uff09 \u8d85\u6297\u539f \u8fd1\u5e74\u6709\u4eba\u8bc1\u5b9e\u8461\u8404\u7403\u83cc\u5185\u6bd2\u7d20\u548c\u94fe\u7403\u83cc\u7ea2\u6591\u6bd2\u7d20\u53ef\u4f5c\u4e3a\u4e00\u79cd \u8d85\u6297\u539f \u542f\u52a8\u6682\u65f6\u6027\u5f02\u5e38\u514d\u75ab\u53cd\u5e94\u3002', 'spo_list': [('\u5ddd\u5d0e\u75c5', '\u53d1\u75c5\u673a\u5236', '\u8d85\u6297\u539f'), ('\u5ddd\u5d0e\u75c5', '\u53d1\u75c5\u673a\u5236', '\u514d\u75ab\u8fc7\u5ea6\u6d3b\u5316')]}, {'text': '\u300a \u5b89\u5f97\u541b\u987e \u300b\u662f\u8fde\u8f7d\u4e8e\u664b\u6c5f\u6587\u5b66\u57ce\u7684\u5c0f\u8bf4\uff0c\u4f5c\u8005\u662f \u5b63\u9759\u597d ', 'spo_list': [('\u5b89\u5f97\u541b\u987e', '\u4f5c\u8005', '\u5b63\u9759\u597d')]}, {'text': '\u7ecf\u6d4e\u65e5\u62a5\u00b7\u4e2d\u56fd\u7ecf\u6d4e\u7f51\u8bb0\u8005\u6768\u9633\u817e\u636e\u4e86\u89e3\uff0c 5\u6761\u57ce\u9645\u94c1\u8def\u603b\u957f\u7ea6410\u516c\u91cc \uff0c\u6295\u8d44 \u7ea62018\u4ebf\u5143 ', 'spo_list': [('5\u6761\u57ce\u9645\u94c1\u8def\u603b\u957f\u7ea6410\u516c\u91cc', '\u6295\u8d44', '\u7ea62018\u4ebf\u5143')]}, {'text': '1\u6c14\u5019 \u5206\u754c\u9547 \u5c5e \u4e9a\u70ed\u5e26\u5b63\u98ce\u6e7f\u6da6\u6c14\u5019 \uff0c\u51ac\u77ed\u590f\u957f\uff0c\u9633\u5149\u5145\u8db3\uff0c\u96e8\u91cf\u5145\u6c9b\uff0c\u5e74\u5e73\u5747\u6c14\u6e2922\u00b0c\uff0c\u964d\u96e8\u91cf1600\u6beb\u7c73\uff0c\u65e0\u971c\u671f345\u5929', 'spo_list': [('\u5206\u754c\u9547', '\u6c14\u5019', '\u4e9a\u70ed\u5e26\u5b63\u98ce\u6e7f\u6da6\u6c14\u5019')]}, {'text': ' \u51b0\u878d\u5316 \u6210 \u6c34 \u540e\uff0c \u6c34 \u7684\u4f53\u79ef\u6bd4 \u51b0\u7684\u4f53\u79ef \u51cf\u5c11\u4e86(1/11)\uff0e\u73b0\u6709\u4e00\u5757\u51b0\uff0c\u878d\u5316\u6210 \u6c34 \u540e\u4f53\u79ef\u662f30dm**3\uff0c\u8fd9\u5757 \u51b0\u7684\u4f53\u79ef \u662f\u591a\u5c11 \u7acb\u65b9\u5398\u7c73 \uff1f', 'spo_list': [('\u51b0\u878d\u5316', '\u53d8\u6210', '\u6c34'), ('\u51b0\u7684\u4f53\u79ef', '\u5355\u4f4d', '\u7acb\u65b9\u5398\u7c73')]}, {'text': '\u201d \u65e5\u672c \u57281964\u5e74\u4e3e\u529e\u4e86 \u4e1c\u4eac\u5965\u8fd0\u4f1a \uff0c\u4e3a\u6b64\uff0c \u65e5\u672c \u5f53\u65f6\u5efa\u6210\u4e86\u5305\u62ec\u65b0\u5e72\u7ebf\u5728\u5185\u7684\u4e00\u7cfb\u5217\u57fa\u7840\u8bbe\u65bd', 'spo_list': [('\u65e5\u672c', '\u4e3e\u529e', '\u4e1c\u4eac\u5965\u8fd0\u4f1a')]}, {'text': ' \u516c\u5b89\u90e8 \u4e13\u95e8\u6210\u7acb \u6253\u51fb\u6574\u6cbb\u4e13\u9879\u884c\u52a8\u9886\u5bfc\u5c0f\u7ec4 \uff0c\u5e76\u7ec4\u7ec7\u6cb3\u5317\u3001\u6e56\u5317\u3001\u798f\u5efa\u7b49\u5730\u516c\u5b89\u6c11\u8b66\u7ec4\u6210\u5de5\u4f5c\u7ec4\uff0c\u8d74\u83f2\u5f8b\u5bbe\u5f00\u5c55\u8b66\u52a1\u6267\u6cd5\u5408\u4f5c\uff0c\u5168\u529b\u4fa6\u529e\u6848\u4ef6', 'spo_list': [('\u516c\u5b89\u90e8', '\u6210\u7acb', '\u6253\u51fb\u6574\u6cbb\u4e13\u9879\u884c\u52a8\u9886\u5bfc\u5c0f\u7ec4')]}, {'text': '\u503c\u5f97\u6ce8\u610f\u7684\u662f, \u4e2d\u592e\u7eaa\u59d4\u56fd\u5bb6\u76d1\u59d4\u7f51\u7ad9 \u8fd8\u516c\u5e03 12\u8d77\u96c6\u4e2d\u901a\u62a5\u6076\u610f\u4e3e\u62a5\u6216\u8bec\u544a\u9677\u5bb3\u5178\u578b\u6848\u4f8b ', 'spo_list': [('\u4e2d\u592e\u7eaa\u59d4\u56fd\u5bb6\u76d1\u59d4\u7f51\u7ad9', '\u516c\u5e03', '12\u8d77\u96c6\u4e2d\u901a\u62a5\u6076\u610f\u4e3e\u62a5\u6216\u8bec\u544a\u9677\u5bb3\u5178\u578b\u6848\u4f8b')]}, {'text': ' dc\u6f2b\u753b\u53cd\u6d3e\u201c\u5c0f\u4e11\u201d\u7684\u72ec\u7acb\u7535\u5f71\u300ajoker\u300b \u5c06\u4e8e 10\u6708 \u4e0a\u6620\uff0c\u9996\u6761\u9884\u544a\u7247\u65e9\u524d\u653e\u51fa\u540e\uff0c\u7537\u4e3b\u89d2\u6770\u6606\u00b7\u83f2\u5c3c\u514b\u65af\uff08joaquinphoenix\uff09\u7684\u6f14\u51fa\u5907\u53d7\u8d5e\u8d4f\uff0c\u66f4\u88ab\u89c6\u4e3a\u660e\u5e74\u5965\u65af\u5361\u5f71\u5e1d\u5927\u70ed\u4eba\u9009\u3002', 'spo_list': [('dc\u6f2b\u753b\u53cd\u6d3e\u201c\u5c0f\u4e11\u201d\u7684\u72ec\u7acb\u7535\u5f71\u300ajoker\u300b', '\u4e0a\u6620', '10\u6708')]}, {'text': '1 \u5468\u5df7\u9547 \u5730\u5904 \u4e9a\u70ed\u5e26\u5b63\u98ce\u6c14\u5019 \u533a\uff0c\u662f\u5168\u56fd\u5c0f\u57ce\u9547\u7efc\u5408\u6539\u9769\u8bd5\u70b9\u9547\u3001\u8054\u5408\u56fd\u8ba1\u5212\u5f00\u53d1\u7f72\u53ef\u6301\u7eed\u53d1\u5c55\u7684\u4e2d\u56fd\u5c0f\u57ce\u9547\u8bd5\u70b9\u95472', 'spo_list': [('\u5468\u5df7\u9547', '\u6c14\u5019', '\u4e9a\u70ed\u5e26\u5b63\u98ce\u6c14\u5019')]}, {'text': ' \u4e2d\u56fd\u5973\u8db3 0\u6bd42\u4e0d \u610f\u5927\u5229 \uff0c\u63d0\u524d\u7ed3\u675f\u4e86\u672c\u6b21\u4e16\u754c\u676f\u7684\u5f81\u7a0b\u3002', 'spo_list': [('\u4e2d\u56fd\u5973\u8db3', '0\u6bd42', '\u610f\u5927\u5229')]}, {'text': '2\u67087\u65e5\uff0c \u6559\u80b2\u90e8 \u7279\u522b\u6307\u51fa\uff0c \u672a\u7ecf\u5b66\u6821\u6279\u51c6\u5b66\u751f\u4e00\u5f8b\u4e0d\u51c6\u8fd4\u6821 \uff0c\u800c\u4e14\u5bf9\u5b66\u751f\u516c\u5bd3\u5b9e\u884c\u5c01\u95ed\u7ba1\u7406', 'spo_list': [('\u6559\u80b2\u90e8', '\u6307\u51fa', '\u672a\u7ecf\u5b66\u6821\u6279\u51c6\u5b66\u751f\u4e00\u5f8b\u4e0d\u51c6\u8fd4\u6821')]}, {'text': '\u65e9\u4e9b\u65f6\u5019 \u56fd\u9645\u7bee\u8054\u4e16\u754c\u676f \u5df2\u7ecf\u843d\u5e55\uff0c\u6700\u7ec8\u7684\u51a0\u519b\u88ab \u897f\u73ed\u7259 \u593a\u5f97\uff0c\u6309\u7406\u8bf4 \u897f\u73ed\u7259 \u593a\u51a0\u540e\u70ed\u5ea6\u5e94\u8be5\u5f88\u9ad8\uff0c\u4f46\u662f\u6839\u636e\u672c\u6b21\u4e16\u754c\u676f\u6765\u770b\uff0c \u897f\u73ed\u7259 \u593a\u51a0\u7684\u70ed\u5ea6\u7adf\u7136\u4e0d\u5982\u7f8e\u56fd\u8f93\u7403\u3002', 'spo_list': [('\u897f\u73ed\u7259', '\u593a\u51a0', '\u56fd\u9645\u7bee\u8054\u4e16\u754c\u676f')]}, {'text': ' 6\u67083\u65e5 \uff0c \u52a0\u62ff\u5927 \u5bf9\u539f\u4ea7\u4e8e\u4e2d\u56fd\u7684\u4e00\u6279\u7ae5\u88c5\u5b9e\u65bd\u4e86\u53ec\u56de\uff0c\u539f\u56e0\u662f\u8fd9\u6279\u7ae5\u88