: [('\u6768\u656c\u8f89', '\u67e5\u623f', '\u8fd9\u4f4d\u75c5\u4eba\u8840\u538b\u5f88\u9ad8')]}, {'text': '\uff082\uff092009\u5e74\uff0c \u4ec1\u5b5d\u96c6\u56e2 \u521b\u59cb\u4eba\u4e4b\u4e00 \u5f20\u9526\u5ddd \u7ec4\u7ec7\u6765\u81ea\u53a6\u95e8\u5404\u9ad8\u6821\u768420\u5b66\u751f\u8d74\u53f0\u6e7e\u4f01\u4e1a\u7814\u4e60\u53c2\u8bbf', 'spo_list': [('\u4ec1\u5b5d\u96c6\u56e2', '\u521b\u59cb\u4eba', '\u5f20\u9526\u5ddd')]}, {'text': ' \u534e\u4e3a \u5728\u7f8e\u56fd\u88c1\u5458 600\u4eba \uff0c8000\u4ebf\u8d44\u672c\u64a4\u79bb\u7f8e\u56fd\uff1a\u4e03\u5927\u5de8\u5934\u8fdb\u8c0f\u7279\u6717\u666e', 'spo_list': [('\u534e\u4e3a', '\u88c1\u5458', '600\u4eba')]}, {'text': '\u827a\u4eba\u7684\u611f\u60c5\u4e8b\u6700\u53d7\u5927\u5bb6\u5173\u6ce8\uff0c \u674e\u5f69\u6866 \u548c \u524d\u7537\u53cb \u76f8\u604b\u4e09\u5e74\u6700\u7ec8\u5206\u5f00\uff0c\u672c\u4ee5\u4e3a\u5979\u4f1a\u7ecf\u5386\u4e00\u6bb5\u611f\u60c5\u4f4e\u8c37\uff0c\u4f46\u662f\u8ba9\u4eba\u610f\u5916\u5979\u5f88\u5feb\u5c31\u627e\u5230\u4e86\u65b0\u7684\u53e6\u4e00\u534a\uff0c\u57282\u6708\u4efd\u7684\u65f6\u5019\u548cerichuang\u7ed3\u5a5a\u3002', 'spo_list': [('\u674e\u5f69\u6866', '\u5206\u5f00', '\u524d\u7537\u53cb')]}, {'text': ' 9\u670823\u65e5 \uff0c 2019\u662d\u901a\u82f9\u679c\u5c55\u9500\u66a8\u201c\u662d\u9633\u7ea2\u201d\u54c1\u724c\u53d1\u5e03\u4f1a \u5f00\u5e55\u5f0f\u5728\u4e91\u5357\u662d\u901a\u56fd\u9645\u4f1a\u8bae\u4f1a\u5c55\u4e2d\u5fc3\u4e3e\u884c\uff0c\u662d\u901a\u5e02\u59d4\u4e66\u8bb0\u6768\u4e9a\u6797\u4e00\u884c\u6df1\u5165\u5404\u4e2a\u5c55\u533a\u5de1\u9986\uff0c\u8be6\u7ec6\u4e86\u89e3\u4f01\u4e1a\u53d1\u5c55\u3001\u5e02\u573a\u8425\u9500\u3001\u57fa\u5730\u5efa\u8bbe\u89c4\u6a21\u7b49\u76f8\u5173\u5de5\u4f5c\u60c5\u51b5\uff0c\u5e76\u73b0\u573a\u4e3a\u662d\u901a\u82f9\u679c\u4ee3\u8a00\u3002', 'spo_list': [('9\u670823\u65e5', '\u5f00\u5e55\u5f0f', '2019\u662d\u901a\u82f9\u679c\u5c55\u9500\u66a8\u201c\u662d\u9633\u7ea2\u201d\u54c1\u724c\u53d1\u5e03\u4f1a')]}, {'text': '\u201d \u6885\u82b1\u6c60\u793e\u533a \u4e66\u8bb0\u53f6\u9707\u4ecb\u7ecd\uff0c\u56e0\u4e3a\u5f53\u5929\u51fa\u73b0\u4e86 \u65e0\u75c7\u72b6\u611f\u67d3\u8005 \uff0c\u793e\u533a\u7acb\u5373\u53d6\u6d88\u4e86\u7fe0\u6797\u96c5\u5c45\u5c0f\u533a\u5c45\u6c11\u6bcf\u6237\u4e09\u5929\u4e00\u6b21\u5916\u51fa\u901a\u884c\u7684\u89c4\u5b9a\uff0c\u672a\u676514\u5929\u91cc\u4ec5\u590d\u5de5\u4eba\u5458\u51ed\u5355\u4f4d\u590d\u5de5\u8bc1\u660e\u52a0\u76d6\u793e\u533a\u516c\u7ae0\uff0c\u4ee5\u53ca\u7eff\u7801\u3001\u6d4b\u6e29\u624d\u53ef\u901a\u884c', 'spo_list': [('\u6885\u82b1\u6c60\u793e\u533a', '\u51fa\u73b0', '\u65e0\u75c7\u72b6\u611f\u67d3\u8005')]}, {'text': ' \u5df4\u57fa\u65af\u5766\u4e00\u65c5\u5ba2\u5217\u8f66 \u8d77\u706b\u81f4 65\u4eba \u6b7b\u4ea1', 'spo_list': [('65\u4eba', '\u8d77\u706b', '\u5df4\u57fa\u65af\u5766\u4e00\u65c5\u5ba2\u5217\u8f66')]}, {'text': '7\u67081\u65e5\uff0c \u6d89\u5acc\u7325\u4eb59\u5c81\u5973\u7ae5\u7684\u65b0\u57ce\u63a7\u80a1\u539f\u8463\u4e8b\u957f\u738b\u632f\u534e \uff0c\u88ab\u4e0a\u6d77 \u8b66\u65b9 \u91c7\u53d6\u5f3a\u5236\u63aa\u65bd\u30022\u65e5\u665a\uff0c\u72af\u7f6a\u5acc\u7591\u4eba\u5468\u67d0\u67d0\u81f3\u516c\u5b89\u673a\u5173\u81ea\u9996\u3002\u76ee\u524d\uff0c\u4e8c\u4eba\u56e0\u6d89\u5acc\u7325\u4eb5\u513f\u7ae5\u7f6a\u5df2\u88ab \u8b66\u65b9 \u5211\u4e8b\u62d8\u7559\uff0c\u6848\u4ef6\u6b63\u5728\u8fdb\u4e00\u6b65\u8c03\u67e5\u4e2d\u3002', 'spo_list': [('\u8b66\u65b9', '\u62d8\u7559', '\u6d89\u5acc\u7325\u4eb59\u5c81\u5973\u7ae5\u7684\u65b0\u57ce\u63a7\u80a1\u539f\u8463\u4e8b\u957f\u738b\u632f\u534e')]}, {'text': ' \u6d69\u5fb7\u63a7\u80a1 \u4ee52240\u4e07\u6e2f\u5143\u6536\u8d2d \u65e5\u672c\u718a\u672c\u5e02\u5546\u4e1a\u533a\u4e00\u7269\u4e1a ', 'spo_list': [('\u6d69\u5fb7\u63a7\u80a1', '\u6536\u8d2d', '\u65e5\u672c\u718a\u672c\u5e02\u5546\u4e1a\u533a\u4e00\u7269\u4e1a')]}, {'text': ' nba\u4e1c\u90e8\u51b3\u8d5b \uff0c \u731b\u9f99 \u529b\u538b\u96c4\u9e7f\uff0c\u518d\u5ea6\u6273\u56de\u4e00\u57ce\uff0c\u5927\u6bd4\u52062-2', 'spo_list': [('nba\u4e1c\u90e8\u51b3\u8d5b', '\u529b\u538b', '\u731b\u9f99')]}, {'text': ' \u5e7b\u7231\u5c9b\u5c7f \u2014\u521b\u59cb\u4eba \u5510\u5f64\u5929 \uff0c\u4e8e2005\u5e7410\u6708\u51ac\u5b63\u4e00\u4e2a\u4eba\u5728\u56fe\u4e66\u9986\u52e4\u5de5\u4fed\u5b66\u65f6\u7684\u521d\u6b65\u60f3\u6cd5', 'spo_list': [('\u5e7b\u7231\u5c9b\u5c7f', '\u521b\u59cb\u4eba', '\u5510\u5f64\u5929')]}, {'text': '\u4eca\u65e5\u662f\u8fd9\u4e48\u591a\u5e74\u5e26\u7ed9\u6211\u4eec\u6b22\u4e50\u7684\u201c\u559c\u5267\u4e4b\u738b\u201d \u5468\u661f\u9a70  57\u5c81 \u751f\u65e5\uff0c\u4f5c\u4e3a\u661f\u5973\u90ce\u7684\u6797\u5141\uff0c\u5df2\u7ecf\u8fde\u7eed\u51e0\u5e74\u4e3a \u5468\u661f\u9a70 \u9001\u4e0a\u795d\u798f\u4e86\uff0c\u8fd9\u6b21\u4e5f\u4e0d\u4f8b\u5916\u3002\u6797\u5141\u4e0a\u4e00\u6b21\u6390\u7740\u96f6\u70b9\u9001\u795d\u798f\u7684\uff0c\u8fd8\u662f\u5979\u7684\u597d\u53cb\u5b8b\u7956\u513f\uff0c\u8fd9\u6b21\u6797\u5141\u540c\u6837\u53d1\u4e86 \u5468\u661f\u9a70 \u4e5d\u5f20\u7167\u7247\uff0c\u5176\u4e2d\u5c31\u6709\u516b\u5f20 \u5468\u661f\u9a70 \u5e74\u8f7b\u65f6\u5019\u7684\u5267\u7167\uff0c\u8fd8\u6709\u4e00\u5f20\u662f\u6797\u5141\u4e13\u95e8p\u8fc7\u7684\u7167\u7247\uff0c\u53ef\u4ee5\u8bf4\u662f\u5f88\u6709\u7231\u7684\u59d1\u5a18\u4e86\u3002', 'spo_list': [('\u5468\u661f\u9a70', '\u751f\u65e5', '57\u5c81')]}, {'text': '\u534e\u6625\u83b9\uff1a\u636e\u5411\u6709\u5173\u90e8\u95e8\u4e86\u89e3\uff0c\u6628\u5929\u508d\u665a7\uff1a30\u5de6\u53f3\uff0c\u5728 \u4e2d\u56fd\u6d59\u6c5f\u821f\u5c71\u4ee5\u4e1c\u4e1c\u6d77\u6d77\u57df \u53d1\u751f\u4e86 \u4e00\u8258\u4e2d\u56fd\u6e14\u8239\u4e0e\u65e5\u672c\u81ea\u536b\u961f\u8230\u8247\u64e6\u78b0\u4e8b\u6545 \uff0c\u5bfc\u81f4\u4e2d\u56fd\u6e14\u8239\u53d7\u635f\uff0c\u4e00\u540d\u6e14\u6c11\u8170\u90e8\u53d7\u4f24', 'spo_list': [('\u4e2d\u56fd\u6d59\u6c5f\u821f\u5c71\u4ee5\u4e1c\u4e1c\u6d77\u6d77\u57df', '\u53d1\u751f', '\u4e00\u8258\u4e2d\u56fd\u6e14\u8239\u4e0e\u65e5\u672c\u81ea\u536b\u961f\u8230\u8247\u64e6\u78b0\u4e8b\u6545')]}, {'text': '8\u67089\u65e5\uff0c\u5170\u9675\u53bf\u516c\u5b89\u5c40\u53d1\u5e03\u901a\u544a\uff0c\u6566\u4fc3\u4efb\u8679\u90e1\u72af\u7f6a\u56e2\u4f19\u6210\u5458\u6295\u6848\u81ea\u9996\u3002\u8fd1\u65e5\uff0c \u5170\u9675\u8b66\u65b9 \u7ecf\u8fc7\u7f1c\u5bc6\u4fa6\u67e5\uff0c\u4fa6\u7834\u4efb\u8679\u90e1\u72af\u7f6a\u56e2\u4f19\u591a\u8d77\u8fdd\u6cd5\u72af\u7f6a\u6848\u4ef6\uff0c\u6293\u83b7 \u4efb\u8679\u90e1\u7b49\u591a\u540d\u72af\u7f6a\u5acc\u7591\u4eba \u3002', 'spo_list': [('\u5170\u9675\u8b66\u65b9', '\u6293\u83b7', '\u4efb\u8679\u90e1\u7b49\u591a\u540d\u72af\u7f6a\u5acc\u7591\u4eba')]}, {'text': ' \u6628\u5929\u4e0a\u5348 \uff0c 2019\u4e2d\u6cd5\u5efa\u7b51\u9057\u4ea7\u4fdd\u62a4\u5fd7\u613f\u8005\u5de5\u4f5c\u8425\u6cb3\u6d25\u8425\u5f00\u8425\u4eea\u5f0f \u5728\u4e0b\u5316\u4e61\u4e0a\u5cad\u6751\u79b9\u738b\u5e99\u4e3e\u884c\uff0c\u526f\u5e02\u957f\u738b\u9ad8\u7ea2\u3001\u6cd5\u56fd\u9057\u4ea7\u4fdd\u62a4\u5fd7\u613f\u8005\u8054\u76df\u4e3b\u5e2d\u5e03\u9c81\u65af\u53c2\u52a0\u5f00\u8425\u4eea\u5f0f', 'spo_list': [('\u6628\u5929\u4e0a\u5348', '\u5f00\u8425', '2019\u4e2d\u6cd5\u5efa\u7b51\u9057\u4ea7\u4fdd\u62a4\u5fd7\u613f\u8005\u5de5\u4f5c\u8425\u6cb3\u6d25\u8425\u5f00\u8425\u4eea\u5f0f')]}, {'text': ' \u6210\u90fd\u4e07\u9686\u4ebf\u5eb7\u533b\u836f\u6709\u9650\u516c\u53f8 \u4e8e 2005\u5e749\u670820\u65e5 \u5728\u6210\u90fd\u5e02\u5de5\u5546\u884c\u653f\u7ba1\u7406\u5c40\u767b\u8bb0\u6210\u7acb', 'spo_list': [('\u6210\u90fd\u4e07\u9686\u4ebf\u5eb7\u533b\u836f\u6709\u9650\u516c\u53f8', '\u6210\u7acb\u65e5\u671f', '2005\u5e749\u670820\u65e5')]}, {'text': '\u5b57\u6bcd\u54e5\u5144\u5f1f\u51e0\u4eba\u90fd\u4f1a\u4ee3\u8868 \u5e0c\u814a\u961f \u53c2\u8d5b\uff0c\u4f5c\u4e3anba\u4eca\u5e74\u7684mvp\u7403\u661f\uff0c\u5b57\u6bcd\u54e5\u53d8\u5f97\u8d8a\u53d1\u7684\u6210\u719f\uff0c\u5728\u8f93\u6389 \u4e1c\u90e8\u51b3\u8d5b \u540e\uff0c\u5b57\u6bcd\u54e5\u6e34\u671b\u81ea\u5df1\u53d8\u5f97\u66f4\u4e3a\u5f3a\u5927\u3002', 'spo_list': [('\u4e1c\u90e8\u51b3\u8d5b', '\u8f93\u6389', '\u5e0c\u814a\u961f')]}, {'text': ' \u8fea\u58eb\u5c3c  106\u4ebf \u5356\u638921\u5bb6\u5730\u65b9\u4f53\u80b2\u7535\u89c6\u7f51\uff0c\u4f46\u89c2\u4f17\u6d41\u5931\u95ee\u9898\u4f9d\u7136\u65e0\u89e3', 'spo_list': [('\u8fea\u58eb\u5c3c', '\u5356\u6389', '106\u4ebf')]}, {'text': '6\u67083\u65e5\u4e0b\u5348\uff0csuperjunior\u7684\u7ecf\u7eaa\u516c\u53f8\u5ba3\u5e03\uff0c sj \u5c06\u5728 \u4e0b\u534a\u5e74 \u53d1\u884c\u7b2c\u4e5d\u5f20\u4e13\u8f91\u3002', 'spo_list': [('\u4e0b\u534a\u5e74', '\u53d1\u884c', 'sj')]}, {'text': '2\u73ed\u5728\u6821\u8868\u73b0\uff1a\u5b66\u751f\u5b66\u4e60\u8f83\u4e3a\u8ba4\u771f\uff0c\u80fd\u591f\u56e2\u7ed3\u540c\u5b66\uff0c\u52a9\u4eba\u4e3a\u4e50\u5730\u5740\uff1a \u7389\u6811 \u53bf\u897f\u676d\u8def\u90ae\u7f16\uff1a 815000 \u5907\u6ce8\uff1a[ \u7389\u6811 \u5730\u9707\u707e\u540e\u4fe1\u606f]\uff1a\u5e73\u5b891\u4e8b\u8ff9\u9752\u6d77 \u7389\u6811 \u7b2c\u4e8c\u6c11\u65cf\u4e2d\u5b66\u7684\u5c15\u739b\u670b\u63aa\u5bb6\u5883\u8d2b\u5bd2\uff0c\u66fe\u4e24\u6b21\u8f8d\u5b66\uff0c\u4f46\u662f\u51ed\u7740\u987d\u5f3a\u7684\u6c42\u5b66\u6b32\u671b\u548c\u5b66\u6821\u7684\u5e2e\u52a9\u91cd\u8fd4\u6821\u56ed', 'spo_list': [('\u7389\u6811', '\u90ae\u653f\u7f16\u7801', '815000')]}, {'text': ' \u8fd1\u65e5 \uff0c\u5b81\u6ce2\u9ad8\u901f\u4e09\u5927\u961f\u7684\u5de1\u903b\u8f66\u63a5\u5230\u6307\u6325\u4e2d\u5fc3\u8b66\u60c5\u79f0\uff1a g92\u676d\u752c\u9ad8\u901f\u5b81\u6ce2\u65b9\u5411254k\u9644\u8fd1\uff08\u725f\u5c71\u4e92\u901a\u9644\u8fd1\uff09 \u53d1\u751f\u4e86\u4e00\u8d77\u662f\u4e8b\u6545\uff0c\u662f\u4e00\u8f86\u9762\u5305\u8f66\u9664\u4e86\u4e8b\u6545\u3002', 'spo_list': [('\u8fd1\u65e5', '\u4e8b\u6545', 'g92\u676d\u752c\u9ad8\u901f\u5b81\u6ce2\u65b9\u5411254k\u9644\u8fd1\uff08\u725f\u5c71\u4e92\u901a\u9644\u8fd1\uff09')]}, {'text': '\u5728\u521a\u7ed3\u675f\u4e0d\u4e45\u7684\u4e00\u573a\u4e52\u4e53\u7403\u6377\u514b\u516c\u5f00\u8d5b\u6df7\u53cc\u9879\u76ee\u5bf9\u51b3\u4e2d\uff0c \u56fd\u4e52\u7ec4\u5408\u6731\u9716\u5cf0/\u4f55\u5353\u4f73 \u906d\u9047\u4e86\u6700\u8fd1\u52bf\u5934\u6b63\u76db\u3001\u624d\u5728\u4fdd\u52a0\u5229\u4e9a\u516c\u5f00\u8d5b\u51fb\u8d25\u4e2d\u56fd\u961f\u593a\u51a0\u7684 \u65e5\u672c\u7ec4\u5408\u6c34\u8c37\u96bc/\u4f0a\u85e4\u7f8e\u8bda \uff0c\u7ed3\u679c\u6211\u4eec\u906d\u9047\u4e860-3\u60e8\u8d25\uff0c\u9057\u61be\u88ab\u6dd8\u6c70\u51fa\u5c40\u3002', 'spo_list': [('\u65e5\u672c\u7ec4\u5408\u6c34\u8c37\u96bc/\u4f0a\u85e4\u7f8e\u8bda', '\u60e8\u8d25', '\u56fd\u4e52\u7ec4\u5408\u6731\u9716\u5cf0/\u4f55\u5353\u4f73')]}, {'text': '\u6295\u8bc9\u91cf\u4e5f\u968f\u4e4b\u589e\u52a0\uff0c 2\u6708\u7684\u6d88\u8d39\u8005\u6295\u8bc9\u91cf \u73af\u6bd4\u589e\u957f 300%\u4ee5\u4e0a ', 'spo_list': [('2\u6708\u7684\u6d88\u8d39\u8005\u6295\u8bc9\u91cf', '\u589e\u957f', '300%\u4ee5\u4e0a')]}, {'text': '1\u670810\u65e5  \u4e2d\u56fd\u79d1\u5b66\u9662\u6b66\u6c49\u75c5\u6bd2\u7814\u7a76\u6240\u7b49\u4e13\u4e1a\u673a\u6784  \u521d\u6b65\u7814\u53d1\u51fa \u68c0\u6d4b\u8bd5\u5242\u76d2 \uff0c \u6b66\u6c49\u5e02 \u7acb\u5373\u7ec4\u7ec7\u5bf9\u5728\u9662\u6536\u6cbb\u7684\u6240\u6709\u76f8\u5173\u75c5\u4f8b\u8fdb\u884c\u6392\u67e5', 'spo_list': [('\u4e2d\u56fd\u79d1\u5b66\u9662\u6b66\u6c49\u75c5\u6bd2\u7814\u7a76\u6240\u7b49\u4e13\u4e1a\u673a\u6784', '\u7814\u53d1', '\u68c0\u6d4b\u8bd5\u5242\u76d2'), ('\u4e2d\u56fd\u79d1\u5b66\u9662\u6b66\u6c49\u75c5\u6bd2\u7814\u7a76\u6240\u7b49\u4e13\u4e1a\u673a\u6784', '\u5730\u70b9', '\u6b66\u6c49\u5e02')]}, {'text': '\u5c31\u5728 \u4eca\u5929 \uff0c\u82f1\u51a0\u4ff1\u4e50\u90e8\u65af\u6258\u514b\u57ce\u5ba3\u5e03\u4e0e \u897f\u73ed\u7259\u7c4d\u7403\u5458\u535a\u626c \u89e3\u7ea6\uff0c\u5728\u53cc\u65b9\u4ecd\u6709\u4e00\u5e74\u5408\u7ea6\u7684\u524d\u63d0\u4e0b\uff0c\u65af\u6258\u514b\u57ce\u4e0d\u60dc\u4ee5\u4e0d\u7ed9\u5176\u5728\u65b0\u8d5b\u5b63\u62a5\u540d\u4e3a\u8981\u631f\uff0c\u5f3a\u884c\u7ed3\u675f\u4e86\u4e0e\u8fd9\u4f4d\u6614\u65e5\u5929\u624d\u7684\u5408\u7ea6\u3